
task build-biquad-filter-node
  base context
  base options

  call make
    bind constructor, text <BiquadFilterNode>
    bind factor, link context
    bind factor, link options
    turn seed

task get-frequency
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <frequency>
    turn seed

task get-detune
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <detune>
    turn seed

task get-q
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <Q>
    turn seed

task get-gain
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <gain>
    turn seed

task get-type
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <type>
    turn seed

task set-type
  base build
  base value

  cause set-aspect
    bind object, link build
    bind aspect, text <type>
    bind factor, link value

task get-frequency-response
  base build
  base frequency-array
  base mag-response-output
  base phase-response-output

  cause call-base
    bind object, link build
    bind method, text <getFrequencyResponse>
    bind factor, link frequency-array
    bind factor, link mag-response-output
    bind factor, link phase-response-output
    turn seed
