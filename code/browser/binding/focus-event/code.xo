
task build-focus-event
  call make
    bind constructor, text <FocusEvent>
    turn seed

task get-related-target
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <relatedTarget>
    turn seed

task set-related-target
  base build
  base value

  cause set-aspect
    bind object, link build
    bind aspect, text <relatedTarget>
    bind factor, link value
