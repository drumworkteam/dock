
task build-idb-version-change-event
  base type
  base event-init-dict

  call make
    bind constructor, text <IDBVersionChangeEvent>
    bind factor, link type
    bind factor, link event-init-dict
    turn seed

task get-old-version
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <oldVersion>
    turn seed

task get-new-version
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <newVersion>
    turn seed
