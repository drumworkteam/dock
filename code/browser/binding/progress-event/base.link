
task build-progress-event
  call make
    bind constructor, text <ProgressEvent>
    turn seed

task get-length-computable
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <lengthComputable>
    turn seed

task get-loaded
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <loaded>
    turn seed

task get-total
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <total>
    turn seed
