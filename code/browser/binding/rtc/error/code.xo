
task build-rtc-error
  base tones
  base duration
  base inter-tone-gap

  call make
    bind constructor, text <RTCError>
    bind factor, link tones
    bind factor, link duration
    bind factor, link inter-tone-gap
    turn seed

task get-error-detail
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <errorDetail>
    turn seed

task get-received-alert
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <receivedAlert>
    turn seed

task get-sctp-cause-code
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <sctpCauseCode>
    turn seed

task get-sdp-line-number
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <sdpLineNumber>
    turn seed

task get-sent-alert
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <sentAlert>
    turn seed
