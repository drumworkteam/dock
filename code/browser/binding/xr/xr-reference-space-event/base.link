
task build-x-r-reference-space-event
  base type
  base event-init-dict

  call make
    bind constructor, text <XRReferenceSpaceEvent>
    bind factor, link type
    bind factor, link event-init-dict
    turn seed

task get-reference-space
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <referenceSpace>
    turn seed

task get-transform
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <transform>
    turn seed
