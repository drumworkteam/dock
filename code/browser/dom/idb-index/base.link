
load @tunebond/bolt/code/javascript
  take form array

load @tunebond/moon
  take form any
  take form or

load @tunebond/bolt/code/javascript/native
  take form native-boolean
  take form native-null
  take form native-number
  take form native-string
  take form native-undefined

load @tunebond/bolt/code/javascript/dom
  take form idb-cursor
  take form idb-cursor-direction
  take form idb-cursor-with-value
  take form idb-key-range
  take form idb-object-store
  take form idb-request
  take form idb-valid-key

form idb-index, name <IDBIndex>
  link key-path, name <keyPath>
    like or
      like native-string
      like array
        like native-string
  link multi-entry, name <multiEntry>
    like native-boolean
  link name, name <name>
    like native-string
    flex true
  link object-store, name <objectStore>
    like idb-object-store
  link unique, name <unique>
    like native-boolean

  task count, name <count>
    like idb-request
      like native-number

    take query
      like or
        like idb-valid-key
        like idb-key-range
      void true

  task get, name <get>
    like idb-request
      like any

    take query
      like or
        like idb-valid-key
        like idb-key-range

  task get-all, name <getAll>
    like idb-request
      like array
        like any

    take query
      like or
        like idb-valid-key
        like idb-key-range
        like native-null
      void true
    take count, like native-number
      void true

  task get-all-keys, name <getAllKeys>
    like idb-request
      like array
        like idb-valid-key

    take query
      like or
        like idb-valid-key
        like idb-key-range
        like native-null
      void true
    take count, like native-number
      void true

  task get-key, name <getKey>
    like idb-request
      like or
        like idb-valid-key
        like native-undefined

    take query
      like or
        like idb-valid-key
        like idb-key-range

  task open-cursor, name <openCursor>
    like idb-request
      like or
        like idb-cursor-with-value
        like native-null

    take query
      like or
        like idb-valid-key
        like idb-key-range
        like native-null
      void true
    take direction, like idb-cursor-direction
      void true

  task open-key-cursor, name <openKeyCursor>
    like idb-request
      like or
        like idb-cursor
        like native-null

    take query
      like or
        like idb-valid-key
        like idb-key-range
        like native-null
      void true
    take direction, like idb-cursor-direction
      void true
