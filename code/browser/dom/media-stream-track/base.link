form media-stream-track, name <MediaStreamTrack>
  base event-target

  link content-hint, name <contentHint>
    like native-string
    flex true
  link enabled, name <enabled>
    like native-boolean
    flex true
  link id, name <id>
    like native-string
  link kind, name <kind>
    like native-string
  link label, name <label>
    like native-string
  link muted, name <muted>
    like native-boolean
  link onended, name <onended>
    like or
      like task
        like any

        take this, like media-stream-track
        take ev, like event
      like native-null
    flex true
  link onmute, name <onmute>
    like or
      like task
        like any

        take this, like media-stream-track
        take ev, like event
      like native-null
    flex true
  link onunmute, name <onunmute>
    like or
      like task
        like any

        take this, like media-stream-track
        take ev, like event
      like native-null
    flex true
  link ready-state, name <readyState>
    like media-stream-track-state

  task apply-constraints, name <applyConstraints>
    like promise
      like native-void

    take constraints, like media-track-constraints
      void true

  task clone, name <clone>
    like media-stream-track

  task get-capabilities, name <getCapabilities>
    like media-track-capabilities

  task get-constraints, name <getConstraints>
    like media-track-constraints

  task get-settings, name <getSettings>
    like media-track-settings

  task stop, name <stop>
    like native-void

  task add-event-listener, name <addEventListener>
    like native-void

    head k
      base key-list
        like media-stream-track-event-map

    take type, like k
    take listener
      like task
        like any

        take this, like media-stream-track
        take ev
          like index
            like media-stream-track-event-map
            like k
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true

  task add-event-listener, name <addEventListener>
    like native-void

    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true

  task remove-event-listener, name <removeEventListener>
    like native-void

    head k
      base key-list
        like media-stream-track-event-map

    take type, like k
    take listener
      like task
        like any

        take this, like media-stream-track
        take ev
          like index
            like media-stream-track-event-map
            like k
    take options
      like or
        like native-boolean
        like event-listener-options
      void true

  task remove-event-listener, name <removeEventListener>
    like native-void

    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
