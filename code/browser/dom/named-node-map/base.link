
load @tunebond/moon
  take form or

load @tunebond/bolt/code/javascript/native
  take form native-null
  take form native-number
  take form native-string

load @tunebond/bolt/code/javascript/dom
  take form attr

form named-node-map, name <NamedNodeMap>
  link length, name <length>
    like native-number

  task get-named-item, name <getNamedItem>
    like or
      like attr
      like native-null

    take qualified-name, like native-string

  task get-named-item-ns, name <getNamedItemNS>
    like or
      like attr
      like native-null

    take namespace
      like or
        like native-string
        like native-null
    take local-name, like native-string

  task item, name <item>
    like or
      like attr
      like native-null

    take index, like native-number

  task remove-named-item, name <removeNamedItem>
    like attr

    take qualified-name, like native-string

  task remove-named-item-ns, name <removeNamedItemNS>
    like attr

    take namespace
      like or
        like native-string
        like native-null
    take local-name, like native-string

  task set-named-item, name <setNamedItem>
    like or
      like attr
      like native-null

    take attr, like attr

  task set-named-item-ns, name <setNamedItemNS>
    like or
      like attr
      like native-null

    take attr, like attr

  mesh index, like native-number
    like attr
