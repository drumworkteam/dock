
load @wavebond/bolt/code/javascript
  take form array

load @wavebond/bolt/code/javascript/native
  take form native-null
  take form native-number
  take form native-string
  take form native-void

load @wavebond/bolt/code/browser/dom
  take form element
  take form html-collection
  take form html-element-tag-name-map
  take form node
  take form node-list-of
  take form svg-element-tag-name-map

form parent-node, name <ParentNode>
  link child-element-count, name <childElementCount>
    like native-number
  link children, name <children>
    like html-collection
  link first-element-child, name <firstElementChild>
    like or
      like element
      like native-null
  link last-element-child, name <lastElementChild>
    like or
      like element
      like native-null

  task append, name <append>
    like native-void

    take nodes
      like array
        like or
          like node
          like native-string
      rest true

  task prepend, name <prepend>
    like native-void

    take nodes
      like array
        like or
          like node
          like native-string
      rest true

  task query-selector, name <querySelector>
    like or
      like index
        like html-element-tag-name-map
        like k
      like native-null

    head k
      base key-list
        like html-element-tag-name-map

    take selectors

  task query-selector, name <querySelector>
    like or
      like index
        like svg-element-tag-name-map
        like k
      like native-null

    head k
      base key-list
        like svg-element-tag-name-map

    take selectors

  task query-selector, name <querySelector>
    like or
      like e
      like native-null

    head e
      base element
      fall element

    take selectors

  task query-selector-all, name <querySelectorAll>
    like node-list-of
      like index
        like html-element-tag-name-map
        like k

    head k
      base key-list
        like html-element-tag-name-map

    take selectors

  task query-selector-all, name <querySelectorAll>
    like node-list-of
      like index
        like svg-element-tag-name-map
        like k

    head k
      base key-list
        like svg-element-tag-name-map

    take selectors

  task query-selector-all, name <querySelectorAll>
    like node-list-of
      like e

    head e
      base element
      fall element

    take selectors

  task replace-children, name <replaceChildren>
    like native-void

    take nodes
      like array
        like or
          like node
          like native-string
      rest true
