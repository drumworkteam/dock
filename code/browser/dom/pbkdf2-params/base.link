
load @tunebond/bolt/code/javascript/native
  take form native-number

load @tunebond/bolt/code/javascript/dom
  take form algorithm
  take form buffer-source
  take form hash-algorithm-identifier

form pbkdf2-params, name <Pbkdf2Params>
  base algorithm

  link hash, name <hash>
    like hash-algorithm-identifier
    flex true
  link iterations, name <iterations>
    like native-number
    flex true
  link salt, name <salt>
    like buffer-source
    flex true
