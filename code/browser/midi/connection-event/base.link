
task build-midi-connection-event
  call make
    bind constructor, text <MIDIConnectionEvent>
    turn seed

task get-port
  base build

  cause get-aspect
    bind object, link build
    bind aspect, text <port>
    turn seed

task set-port
  base build
  base value

  cause set-aspect
    bind object, link build
    bind aspect, text <port>
    bind factor, link value
