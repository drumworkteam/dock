
load @tunebond/bolt/code/javascript/native
  take form native-boolean
  take form native-null
  take form native-string

load @tunebond/bolt/code/browser/webworker
  take form blob
  take form form-data
  take form readable-stream

load @tunebond/bolt/code/javascript/es5
  take form array-buffer
  take form promise
  take form uint8-array

load @tunebond/bolt/code/browser/dom/body
  take form body

form body, name <Body>
  link body, name <body>
    like or
      like readable-stream
        like uint8-array
      like native-null
  link body-used, name <bodyUsed>
    like native-boolean

  task array-buffer, name <arrayBuffer>
    like promise
      like array-buffer

  task blob, name <blob>
    like promise
      like blob

  task form-data, name <formData>
    like promise
      like form-data

  task json, name <json>
    like promise
      like any

  task text, name <text>
    like promise
      like native-string
