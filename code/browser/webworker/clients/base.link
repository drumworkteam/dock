
load @nerdbond/bolt/code/javascript/native
  take form native-null
  take form native-string
  take form native-undefined
  take form native-void

load @nerdbond/bolt/code/browser/webworker
  take form client
  take form client-query-options
  take form window-client

load @nerdbond/bolt/code/javascript/es5
  take form promise
  take form readonly-array

form clients, name <Clients>
  task claim, name <claim>
    like promise
      like native-void

  task get, name <get>
    like promise
      like or
        like client
        like native-undefined

    take id

  task match-all, name <matchAll>
    like promise
      like readonly-array
        like test
          fork roll
            like index
              like t
              text <type>
            text <window>
            hook true
              like window-client
            hook false
              like client

    head t
      base client-query-options

    take options, like maybe, like t

  task open-window, name <openWindow>
    like promise
      like or
        like window-client
        like native-null

    take url
