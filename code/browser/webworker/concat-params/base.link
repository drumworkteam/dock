
load @textsurf/bolt/code/javascript/native
  take form native-string

load @textsurf/bolt/code/browser/webworker
  take form algorithm

load @textsurf/bolt/code/javascript/es5
  take form uint8-array

form concat-params, name <ConcatParams>
  base algorithm

  link algorithm-id, name <algorithmId>
    like uint8-array
    flex true
  link hash, name <hash>
    like or
      like native-string
      like algorithm
    flex true
  link party-u-info, name <partyUInfo>
    like uint8-array
    flex true
  link party-v-info, name <partyVInfo>
    like uint8-array
    flex true
  link private-info, name <privateInfo>
    like uint8-array
    flex true
  link public-info, name <publicInfo>
    like uint8-array
    flex true
