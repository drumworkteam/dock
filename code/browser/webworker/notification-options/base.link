
load @nerdbond/bolt/code/javascript
  take form array

load @nerdbond/bolt/code/javascript/native
  take form native-boolean
  take form native-number
  take form native-string

load @nerdbond/bolt/code/browser/webworker
  take form notification-action
  take form notification-direction
  take form vibrate-pattern

load @nerdbond/bolt/code/browser/dom/notification-options
  take form notification-options

form notification-options, name <NotificationOptions>
  link actions, name <actions>
    like array
      like notification-action
    flex true
  link badge, name <badge>
    like native-string
    flex true
  link body, name <body>
    like native-string
    flex true
  link data, name <data>
    like any
    flex true
  link dir, name <dir>
    like notification-direction
    flex true
  link icon, name <icon>
    like native-string
    flex true
  link image, name <image>
    like native-string
    flex true
  link lang, name <lang>
    like native-string
    flex true
  link renotify, name <renotify>
    like native-boolean
    flex true
  link require-interaction, name <requireInteraction>
    like native-boolean
    flex true
  link silent, name <silent>
    like native-boolean
    flex true
  link tag, name <tag>
    like native-string
    flex true
  link timestamp, name <timestamp>
    like native-number
    flex true
  link vibrate, name <vibrate>
    like vibrate-pattern
    flex true
