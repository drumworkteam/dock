
load @tunebond/moon
  take form any
  take form index
  take form key-list
  take form like
  take form or
  take form task

load @tunebond/bolt/code/javascript/native
  take form native-boolean
  take form native-null
  take form native-number
  take form native-string
  take form native-void

load @tunebond/bolt/code/javascript/webworker
  take form add-event-listener-options
  take form event
  take form event-listener-options
  take form event-listener-or-event-listener-object
  take form event-target
  take form performance-entry-list
  take form performance-event-map

load @tunebond/bolt/code/javascript/dom/performance
  take form performance

form performance, name <Performance>
  base event-target

  link onresourcetimingbufferfull, name <onresourcetimingbufferfull>
    like or
      like task
        like any

        take this, like performance
        take ev, like event
      like native-null
    flex true
  link time-origin, name <timeOrigin>
    like native-number

  task clear-marks, name <clearMarks>
    like native-void

    take mark-name, like native-string
      void true

  task clear-measures, name <clearMeasures>
    like native-void

    take measure-name, like native-string
      void true

  task clear-resource-timings, name <clearResourceTimings>
    like native-void

  task get-entries, name <getEntries>
    like performance-entry-list

  task get-entries-by-name, name <getEntriesByName>
    like performance-entry-list

    take name, like native-string
    take type, like native-string
      void true

  task get-entries-by-type, name <getEntriesByType>
    like performance-entry-list

    take type, like native-string

  task mark, name <mark>
    like native-void

    take mark-name, like native-string

  task measure, name <measure>
    like native-void

    take measure-name, like native-string
    take start-mark, like native-string
      void true
    take end-mark, like native-string
      void true

  task now, name <now>
    like native-number

  task set-resource-timing-buffer-size, name <setResourceTimingBufferSize>
    like native-void

    take max-size, like native-number

  task to-json, name <toJSON>
    like any

  task add-event-listener, name <addEventListener>
    like native-void

    head k
      base key-list
        like performance-event-map

    take type, like k
    take listener
      like task
        like any

        take this, like performance
        take ev
          like index
            like performance-event-map
            like k
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true

  task add-event-listener, name <addEventListener>
    like native-void

    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true

  task remove-event-listener, name <removeEventListener>
    like native-void

    head k
      base key-list
        like performance-event-map

    take type, like k
    take listener
      like task
        like any

        take this, like performance
        take ev
          like index
            like performance-event-map
            like k
    take options
      like or
        like native-boolean
        like event-listener-options
      void true

  task remove-event-listener, name <removeEventListener>
    like native-void

    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
