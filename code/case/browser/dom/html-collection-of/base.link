
load @tunebond/moon
  take form like
  take form or

load @tunebond/bolt/code/javascript/native
  take form native-null
  take form native-number
  take form native-string

load @tunebond/bolt/code/javascript/dom
  take form element
  take form html-collection-base

form html-collection-of, name <HTMLCollectionOf>
  base html-collection-base

  head t
    base element

  task item, name <item>
    like or
      like t
      like native-null

    take index, like native-number

  task named-item, name <namedItem>
    like or
      like t
      like native-null

    take name, like native-string

  mesh index, like native-number
    like t
