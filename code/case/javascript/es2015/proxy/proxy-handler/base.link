
load @tunebond/bolt/code/javascript
  take form array

load @tunebond/moon
  take form any
  take form like
  take form or

load @tunebond/bolt/code/javascript/native
  take form native-boolean
  take form native-null
  take form native-object
  take form native-string
  take form native-symbol
  take form native-undefined

load @tunebond/bolt/code/javascript/es2015/core
  take form function

load @tunebond/bolt/code/javascript/es5
  take form array-like
  take form property-descriptor

form proxy-handler, name <ProxyHandler>
  head t
    base native-object

  task apply, name <apply>
    like any

    take target, like t
    take this-arg, like any
    take arg-array
      like array
        like any

  task construct, name <construct>
    like native-object

    take target, like t
    take arg-array
      like array
        like any
    take new-target, like function

  task define-property, name <defineProperty>
    like native-boolean

    take target, like t
    take p
      like or
        like native-string
        like native-symbol
    take attributes, like property-descriptor

  task delete-property, name <deleteProperty>
    like native-boolean

    take target, like t
    take p
      like or
        like native-string
        like native-symbol

  task get, name <get>
    like any

    take target, like t
    take p
      like or
        like native-string
        like native-symbol
    take receiver, like any

  task get-own-property-descriptor, name <getOwnPropertyDescriptor>
    like or
      like property-descriptor
      like native-undefined

    take target, like t
    take p
      like or
        like native-string
        like native-symbol

  task get-prototype-of, name <getPrototypeOf>
    like or
      like native-object
      like native-null

    take target, like t

  task has, name <has>
    like native-boolean

    take target, like t
    take p
      like or
        like native-string
        like native-symbol

  task is-extensible, name <isExtensible>
    like native-boolean

    take target, like t

  task own-keys, name <ownKeys>
    like array-like
      like or
        like native-string
        like native-symbol

    take target, like t

  task prevent-extensions, name <preventExtensions>
    like native-boolean

    take target, like t

  task set, name <set>
    like native-boolean

    take target, like t
    take p
      like or
        like native-string
        like native-symbol
    take value, like any
    take receiver, like any

  task set-prototype-of, name <setPrototypeOf>
    like native-boolean

    take target, like t
    take v
      like or
        like native-object
        like native-null
