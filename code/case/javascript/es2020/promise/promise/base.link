
load @tunebond/bolt/code/javascript
  take form array

load @tunebond/moon
  take form index
  take form key-list
  take form like
  take form or
  take form test
  take form tuple
  take form unknown

load @tunebond/bolt/code/javascript/es2015/iterable
  take form iterable

load @tunebond/bolt/code/javascript/es2020/promise
  take form promise-settled-result

load @tunebond/bolt/code/javascript/es5
  take form promise-like

load @tunebond/bolt/code/javascript/native
  take form u

load @tunebond/bolt/code/javascript/es2018/promise/promise
  take form promise

form promise
  hook self
    task all-settled, name <allSettled>
      like promise
        slot self
        walk link-name-like-list
          loan
          like key-list
            like t
          take name
          take p
          beam self
            take {name}
              like promise-settled-result
                like test
                  fork roll
                    like index
                      like t
                      like p
                    like promise-like
                      name u
                    hook true
                      like u
                    hook false
                      like index
                        like t
                        like p

      head t
        base or
          like array
            like unknown
          like tuple
            like unknown

      take values, like t

    task all-settled, name <allSettled>
      like promise
        like array
          like promise-settled-result
            like test
              fork roll
                like t
                like promise-like
                  name u
                hook true
                  like u
                hook false
                  like t

      head t

      take values
        like iterable
          like t
