
load @drumwork/seed/code/form/task
  take form task

load @drumwork/seed/code/form/boolean
  take form boolean

cast boolean-task
  bear boolean

cast void-task

task walk
  link check, cast boolean-task
  link block, cast void-task

  call loop
    bind check, loan check
    bind block, loan block

task check
  link check, cast boolean-task
  link block, cast void-task

  call test
    bind check, loan check
    bind block, loan block

task check-else
  link check, cast boolean-task
  link block, cast void-task
  link other, cast void-task

  call test-else
    bind check, loan check
    bind block, loan block
    bind else, loan other

task debug-function
  link func, form task

  call call-base
    bind object, text <window>
    bind method, text <debug>
    bind factor, loan func

task debug
  call debug
    bind keyword, text <debugger>

task queue
  link func, form task

  call call-base
    bind object, text <window>
    bind method, text <setImmediate>
    bind factor, loan func
    turn seed

task compute-bitwise-or
  link left
  link right

  call call-twin
    bind left, loan left
    bind operation, text <|>
    bind right, loan right
    turn seed

task check-if-equal
  link left
  link right

  call call-twin
    bind left, loan left
    bind operation, text <==>
    bind right, loan right
    turn seed

task check-if-null
  link value

  call call-twin
    bind left, loan value
    bind operation, text <==>
    bind right, text <null>
    turn seed

task check-if-strictly-equal
  link left
  link right

  call call-twin
    bind left, loan left
    bind operation, text <===>
    bind right, loan right
    turn seed

task get-typeof
  link value

  call call-keyword
    bind keyword, text <typeof>
    bind value, loan value
    turn seed

task get-instanceof
  link left
  link right

  call call-keyword-2
    bind left, loan left
    bind keyword, text <instanceof>
    bind right, loan right
    turn seed

task set-field
  link object
  link attribute, form chord
  link value

  call set-dynamic-aspect
    bind object, loan object
    bind aspect, loan attribute
    bind factor, loan value

task get-field
  link object
  link attribute, form chord

  call get-dynamic-aspect
    bind object, loan object
    bind aspect, loan attribute
    turn seed

task remove-field
  link object
  link attribute, form chord

  call delete
    bind object, loan object
    bind aspect, loan attribute

