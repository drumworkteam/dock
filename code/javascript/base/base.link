  link right

  call call-twin
    bind left, loan left
    bind operation, text <\<\<>
    bind right, loan right
    turn seed

task shift-right
  link left
  link right

  call call-twin
    bind left, loan left
    bind operation, text <\>\>>
    bind right, loan right
    turn seed

task shift-right-unsigned
  link left
  link right

  call call-twin
    bind left, loan left
    bind operation, text <\>\>\>>
    bind right, loan right
