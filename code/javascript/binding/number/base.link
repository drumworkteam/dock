
fetch ..
  fetch /string
    catch form string
  fetch /function
    catch form function

field number

field integer
  form number

field integer-32
  form integer

field decimal

field unsigned-integer
  form integer

task parse-decimal
  base string, form string

  call call-method
    bind object, <window>
    bind method, <parseFloat>
    bind factor, link string
    turn seed

task parse-int
  base string, form string

  call call-method
    bind object, <window>
    bind method, <parseInt>
    bind factor, link string
    turn seed

task parse-number
  base value

  call call-method
    bind object, <window>
    bind method, <Number>
    bind factor, link value
    turn seed

task get-max
  base start, form number
  base end, form number

  call call-method
    bind object, <Math>
    bind method, <max>
    bind factor, link start
    bind factor, link end
    turn seed

task get-min
  base start, form number
  base end, form number

  call call-method
