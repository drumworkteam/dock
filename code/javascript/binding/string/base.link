
fetch ../number
  catch form unsigned-integer
  catch form integer

field string

task replace
  base string, form string
  base pattern
  base replacer

  call call-method
    bind object, link string
    bind method, <replace>
    bind factor, link pattern
    bind factor, link replacer
    turn seed

task trim
  base string, form string

  call call-method
    bind object, link string
    bind method, <trim>
    turn seed

task get-char-code-at
  base string, form string
  base index, form unsigned-integer

  call call-method
    bind object, link string
    bind method, <charCodeAt>
    bind factor, link index
    turn seed

task get-char-at
  base string, form string
  base index, form unsigned-integer

  call call-method
    bind object, link string
    bind method, <charAt>
    bind factor, link index
    turn seed

task get-char-from-code
  base code, form integer

  call call-method
    bind object, <String>
    bind method, <fromCharCode>
    bind factor, link code
    turn seed

task get-char-from-code-point
  base code, form integer

  call call-method
    bind object, <String>
    bind method, <fromCodePoint>
    bind factor, link code
    turn seed

task convert-to-lowercase
  base string

  call call-method
    bind object, link string
    bind method, <toLowerCase>
    turn seed

task convert-to-uppercase
  base string

  call call-method
    bind object, link string
    bind method, <toUpperCase>
    turn seed

task create
  call create-literal
    bind literal, <''>
    turn seed

task match
  base string
  base pattern

  call call-method
    bind object, link string
    bind method, <match>
    bind factor, link pattern
    turn seed

task create-collator
  call invoke-constructor
    bind constructor, <Intl.Collator>

task get-collator-comparator
  base collator
  call get-aspect
    bind object, link collator
    bind aspect, <compare>

task split
  base string
  base pattern

  call call-method
    bind object, link string
    bind method, <split>
    bind factor, link pattern
    turn seed

task check-starts-with
  base string
  base pattern

  call call-method
    bind object, link string
    bind method, <startsWith>
    bind factor, link pattern
    turn seed

task check-ends-with
  base string
  base pattern

  call call-method
    bind object, link string
    bind method, <endsWith>
    bind factor, link pattern
    turn seed

task pad-start
  base string
  base pad-size
  base pad-string

  call call-method
    bind object, link string
    bind method, <padStart>
    bind factor, link pad-size
    bind factor, link pad-string
    turn seed
