
load @tunebond/bolt/code/javascript/native
  take face native-boolean

load @tunebond/bolt/code/javascript/dom
  take face abort-signal
  take face event-listener-options

face add-event-listener-options, name <AddEventListenerOptions>
  base event-listener-options

  link once, name <once>
    like native-boolean
    flex true
  link passive, name <passive>
    like native-boolean
    flex true
  link signal, name <signal>
    like abort-signal
    flex true
