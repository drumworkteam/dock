
load @tunebond/bolt/code/javascript/native
  take face native-number

load @tunebond/bolt/code/javascript/dom
  take face algorithm
  take face buffer-source

face aes-gcm-params, name <AesGcmParams>
  base algorithm

  link additional-data, name <additionalData>
    like buffer-source
    flex true
  link iv, name <iv>
    like buffer-source
    flex true
  link tag-length, name <tagLength>
    like native-number
    flex true
