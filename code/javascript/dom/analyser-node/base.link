
load @tunebond/nest/code/javascript/native
  take face native-number
  take face native-void

load @tunebond/nest/code/javascript/dom
  take face audio-node

load @tunebond/nest/code/javascript/es5
  take face float32-array
  take face uint8-array

face analyser-node, name <AnalyserNode>
  base audio-node

  link fft-size, name <fftSize>
    like native-number
    flex true
  link frequency-bin-count, name <frequencyBinCount>
    like native-number
  link max-decibels, name <maxDecibels>
    like native-number
    flex true
  link min-decibels, name <minDecibels>
    like native-number
    flex true
  link smoothing-time-constant, name <smoothingTimeConstant>
    like native-number
    flex true

  task get-byte-frequency-data, name <getByteFrequencyData>
    take array, like uint8-array
    free seed, like native-void

  task get-byte-time-domain-data, name <getByteTimeDomainData>
    take array, like uint8-array
    free seed, like native-void

  task get-float-frequency-data, name <getFloatFrequencyData>
    take array, like float32-array
    free seed, like native-void

  task get-float-time-domain-data, name <getFloatTimeDomainData>
    take array, like float32-array
    free seed, like native-void
