
load @tunebond/bolt/code/javascript/native
  take face native-number

load @tunebond/bolt/code/javascript/dom
  take face audio-node-options

face analyser-options, name <AnalyserOptions>
  base audio-node-options

  link fft-size, name <fftSize>
    like native-number
    flex true
  link max-decibels, name <maxDecibels>
    like native-number
    flex true
  link min-decibels, name <minDecibels>
    like native-number
    flex true
  link smoothing-time-constant, name <smoothingTimeConstant>
    like native-number
    flex true
