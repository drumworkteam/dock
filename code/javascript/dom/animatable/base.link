
load @treesurf/seed/code/javascript/base/native-array
  take form native-array

load @treesurf/seed/code/javascript/base/native-null
  take form native-null

load @treesurf/seed/code/javascript/base/native-number
  take form native-number

load @treesurf/seed/code/javascript/dom/animation
  take form animation

load @treesurf/seed/code/javascript/dom/get-animations-options
  take form get-animations-options

load @treesurf/seed/code/javascript/dom/keyframe
  take form keyframe

load @treesurf/seed/code/javascript/dom/keyframe-animation-options
  take form keyframe-animation-options

load @treesurf/seed/code/javascript/dom/property-indexed-keyframes
  take form property-indexed-keyframes

load @treesurf/seed/code/form/or
  take form or
    save form or-link-text

form animatable, name <Animatable>
  task animate, name <animate>
    take keyframes
      like or-link-text
        like native-array
          like keyframe
        like property-indexed-keyframes
        like native-null
    take options
      like or-link-text
        like native-number
        like keyframe-animation-options
      void true
    free seed, like animation

  task get-animations, name <getAnimations>
    take options, like get-animations-options
      void true
    free seed
      like native-array
        like animation
