
load @treesurf/nest/code/javascript/native
  take face native-number
  take face native-void

load @treesurf/nest/code/javascript/es5
  take face float32-array

face audio-buffer, name <AudioBuffer>
  link duration, name <duration>
    like native-number
  link length, name <length>
    like native-number
  link number-of-channels, name <numberOfChannels>
    like native-number
  link sample-rate, name <sampleRate>
    like native-number

  task copy-from-channel, name <copyFromChannel>
    take destination, like float32-array
    take channel-number, like native-number
    take buffer-offset, like native-number
      void true
    free seed, like native-void

  task copy-to-channel, name <copyToChannel>
    take source, like float32-array
    take channel-number, like native-number
    take buffer-offset, like native-number
      void true
    free seed, like native-void

  task get-channel-data, name <getChannelData>
    take channel, like native-number
    free seed, like float32-array
