
load @tunebond/moon
  take suit any

load @tunebond/bolt/code/javascript
  take face array

load @tunebond/bolt/code/javascript/native
  take face native-number
  take face native-string

load @tunebond/bolt/code/javascript/dom
  take face audio-node-options

load @tunebond/bolt/code/javascript/es5
  take face record

face audio-worklet-node-options, name <AudioWorkletNodeOptions>
  base audio-node-options

  link number-of-inputs, name <numberOfInputs>
    like native-number
    flex true
  link number-of-outputs, name <numberOfOutputs>
    like native-number
    flex true
  link output-channel-count, name <outputChannelCount>
    like array
      like native-number
    flex true
  link parameter-data, name <parameterData>
    like record
      like native-string
      like native-number
    flex true
  link processor-options, name <processorOptions>
    like any
    flex true
