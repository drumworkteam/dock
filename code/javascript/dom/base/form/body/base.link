
load @drumwork/land/code/javascript/base/form/native-any
  take form native-any

load @drumwork/land/code/javascript/base/form/native-boolean
  take form native-boolean

load @drumwork/land/code/javascript/base/form/native-null
  take form native-null

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/dom/base/form/blob
  take form blob

load @drumwork/land/code/javascript/dom/base/form/form-data
  take form form-data

load @drumwork/land/code/javascript/dom/base/form/readable-stream
  take form readable-stream

load @drumwork/land/code/javascript/es5/base/form/array-buffer
  take form array-buffer

load @drumwork/land/code/javascript/es5/base/form/promise
  take form promise

load @drumwork/land/code/javascript/es5/base/form/uint8-array
  take form uint8-array

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form body
  take body
    like sink-link-text
      like readable-stream
        like uint8-array
      like native-null
  take body-used, like native-boolean

  task array-buffer
    like task
      free seed
        like promise
          like array-buffer

  task blob
    like task
      free seed
        like promise
          like blob

  task form-data
    like task
      free seed
        like promise
          like form-data

  task json
    like task
      free seed
        like promise
          like native-any

  task text
    like task
      free seed
        like promise
          like native-string
