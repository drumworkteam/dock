
load @drumwork/land/code/javascript/base/form/native-any
  take form native-any

load @drumwork/land/code/javascript/base/form/native-null
  take form native-null

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/dom/base/form/document
  take form document

load @drumwork/land/code/javascript/dom/base/form/document-type
  take form document-type

load @drumwork/land/code/javascript/dom/base/form/xml-document
  take form xml-document

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form dom-implementation
  task create-document
    like task
      take namespace
        like sink-link-text
          like native-string
          like native-null
      take qualified-name
        like sink-link-text
          like native-string
          like native-null
      take doctype
        like sink-link-text
          like document-type
          like native-null
        void take
      free seed, like xml-document

  task create-document-type
    like task
      take qualified-name, like native-string
      take public-id, like native-string
      take system-id, like native-string
      free seed, like document-type

  task create-html-document
    like task
      take title, like native-string
        void take
      free seed, like document

  task has-feature
    like task
      take args
        like list-link-text
          like native-any
        rest take
      free seed, text <true>
