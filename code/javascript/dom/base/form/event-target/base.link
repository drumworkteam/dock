
load @drumwork/land/code/javascript/base/form/native-boolean
  take form native-boolean

load @drumwork/land/code/javascript/base/form/native-null
  take form native-null

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/base/form/native-void
  take form native-void

load @drumwork/land/code/javascript/dom/base/form/add-event-listener-options
  take form add-event-listener-options

load @drumwork/land/code/javascript/dom/base/form/event
  take form event

load @drumwork/land/code/javascript/dom/base/form/event-listener-options
  take form event-listener-options

load @drumwork/land/code/javascript/dom/base/form/event-listener-or-event-listener-object
  take form event-listener-or-event-listener-object

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form event-target
  task add-event-listener
    like task
      take type, like native-string
      take callback
        like sink-link-text
          like event-listener-or-event-listener-object
          like native-null
      take options
        like sink-link-text
          like add-event-listener-options
          like native-boolean
        void take
      free seed, like native-void

  task dispatch-event
    like task
      take event, like event
      free seed, like native-boolean

  task remove-event-listener
    like task
      take type, like native-string
      take callback
        like sink-link-text
          like event-listener-or-event-listener-object
          like native-null
      take options
        like sink-link-text
          like event-listener-options
          like native-boolean
        void take
      free seed, like native-void
