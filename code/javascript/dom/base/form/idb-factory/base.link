
load @drumwork/land/code/javascript/base/form/native-any
  take form native-any

load @drumwork/land/code/javascript/base/form/native-number
  take form native-number

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/dom/base/form/idb-database-info
  take form idb-database-info

load @drumwork/land/code/javascript/dom/base/form/idb-open-d-b-request
  take form idb-open-d-b-request

load @drumwork/land/code/javascript/es5/base/form/promise
  take form promise

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

form idb-factory
  task cmp
    like task
      take first, like native-any
      take second, like native-any
      free seed, like native-number

  task databases
    like task
      free seed
        like promise
          like list-link-text
            like idb-database-info

  task delete-database
    like task
      take name, like native-string
      free seed, like idb-open-d-b-request

  task open
    like task
      take name, like native-string
      take version, like native-number
        void take
      free seed, like idb-open-d-b-request
