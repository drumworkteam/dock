
load @drumwork/land/code/javascript/base/form/native-null
  take form native-null

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/dom/base/form/mutation-record-type
  take form mutation-record-type

load @drumwork/land/code/javascript/dom/base/form/node
  take form node

load @drumwork/land/code/javascript/dom/base/form/node-list
  take form node-list

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form mutation-record
  take added-nodes, like node-list
  take attribute-name
    like sink-link-text
      like native-string
      like native-null
  take attribute-namespace
    like sink-link-text
      like native-string
      like native-null
  take next-sibling
    like sink-link-text
      like node
      like native-null
  take old-value
    like sink-link-text
      like native-string
      like native-null
  take previous-sibling
    like sink-link-text
      like node
      like native-null
  take removed-nodes, like node-list
  take target, like node
  take type, like mutation-record-type
