
form svg-angle, text <SVGAngle>
  take unit-type, text <unitType>
  take value, text <value>
  take value-as-string, text <valueAsString>
  take value-in-specified-units, text <valueInSpecifiedUnits>
  take svg-angletype-deg, text <SVG_ANGLETYPE_DEG>
  take svg-angletype-grad, text <SVG_ANGLETYPE_GRAD>
  take svg-angletype-rad, text <SVG_ANGLETYPE_RAD>
  take svg-angletype-unknown, text <SVG_ANGLETYPE_UNKNOWN>
  take svg-angletype-unspecified, text <SVG_ANGLETYPE_UNSPECIFIED>

  task convert-to-specified-units, text <convertToSpecifiedUnits>
  task new-value-specified-units, text <newValueSpecifiedUnits>