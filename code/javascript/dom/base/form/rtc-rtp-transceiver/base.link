
load @drumwork/land/code/javascript/base/form/native-null
  take form native-null

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/base/form/native-void
  take form native-void

load @drumwork/land/code/javascript/dom/base/form/rtc-rtp-receiver
  take form rtc-rtp-receiver

load @drumwork/land/code/javascript/dom/base/form/rtc-rtp-sender
  take form rtc-rtp-sender

load @drumwork/land/code/javascript/dom/base/form/rtc-rtp-transceiver-direction
  take form rtc-rtp-transceiver-direction

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form rtc-rtp-transceiver
  take current-direction
    like sink-link-text
      like rtc-rtp-transceiver-direction
      like native-null
  take direction, like rtc-rtp-transceiver-direction
    lock free
  take mid
    like sink-link-text
      like native-string
      like native-null
  take receiver, like rtc-rtp-receiver
  take sender, like rtc-rtp-sender

  task stop
    like task
      free seed, like native-void
