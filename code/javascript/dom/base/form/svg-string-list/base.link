
load @drumwork/land/code/javascript/base/form/native-number
  take form native-number

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/base/form/native-void
  take form native-void

form svg-string-list
  take length, like native-number
  take number-of-items, like native-number

  task append-item
    like task
      take new-item, like native-string
      free seed, like native-string

  task clear
    like task
      free seed, like native-void

  task get-item
    like task
      take index, like native-number
      free seed, like native-string

  task initialize
    like task
      take new-item, like native-string
      free seed, like native-string

  task insert-item-before
    like task
      take new-item, like native-string
      take index, like native-number
      free seed, like native-string

  task remove-item
    like task
      take index, like native-number
      free seed, like native-string

  task replace-item
    like task
      take new-item, like native-string
      take index, like native-number
      free seed, like native-string

  cite index, like native-number
    like native-string
