      lock free
    task new
      like task
        take init
          like sink-link-text
            like native-string
            like list-link-text
              like native-number
          void take
        free seed, like dom-matrix
  
    task from-float32-array
      like task
        take array32, like float32-array
        free seed, like dom-matrix
  
    task from-float64-array
      like task
        take array64, like float64-array
        free seed, like dom-matrix
  
    task from-matrix
      like task
        take other, like dom-matrix-init
          void take
        free seed, like dom-matrix
  