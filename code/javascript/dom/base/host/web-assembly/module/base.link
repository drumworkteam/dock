
load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/dom/base/form/buffer-source
  take form buffer-source

load @drumwork/land/code/javascript/dom/base/form/web-assembly/module
  take form module

load @drumwork/land/code/javascript/dom/base/form/web-assembly/module-export-descriptor
  take form module-export-descriptor

load @drumwork/land/code/javascript/dom/base/form/web-assembly/module-import-descriptor
  take form module-import-descriptor

load @drumwork/land/code/javascript/es5/base/form/array-buffer
  take form array-buffer

load @drumwork/seed/code/form/form
  take form form
    save form form-link-text

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

host module
  like form-link-text
    take prototype, like module
      lock free
    task new
      like task
        take bytes, like buffer-source
        free seed, like module
  
    task custom-sections
      like task
        take module-object, like module
        take section-name, like native-string
        free seed
          like list-link-text
            like array-buffer
  
    task exports
      like task
        take module-object, like module
        free seed
          like list-link-text
            like module-export-descriptor
  
    task imports
      like task
        take module-object, like module
        free seed
          like list-link-text
            like module-import-descriptor
  