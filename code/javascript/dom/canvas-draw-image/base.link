
load @tunebond/bolt/code/javascript/native
  take face native-number
  take face native-void

load @tunebond/bolt/code/javascript/dom
  take face canvas-image-source

face canvas-draw-image, name <CanvasDrawImage>
  task draw-image, name <drawImage>
    take image, like canvas-image-source
    take dx, like native-number
    take dy, like native-number
    free seed, like native-void

  task draw-image, name <drawImage>
    take image, like canvas-image-source
    take dx, like native-number
    take dy, like native-number
    take dw, like native-number
    take dh, like native-number
    free seed, like native-void

  task draw-image, name <drawImage>
    take image, like canvas-image-source
    take sx, like native-number
    take sy, like native-number
    take sw, like native-number
    take sh, like native-number
    take dx, like native-number
    take dy, like native-number
    take dw, like native-number
    take dh, like native-number
    free seed, like native-void
