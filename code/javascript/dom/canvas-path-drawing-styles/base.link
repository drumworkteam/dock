
load @drumwork/land/code/javascript/base/native-number
  take form native-number

load @drumwork/land/code/javascript/base/native-void
  take form native-void

load @drumwork/land/code/javascript/dom/canvas-line-cap
  take form canvas-line-cap

load @drumwork/land/code/javascript/dom/canvas-line-join
  take form canvas-line-join

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

form canvas-path-drawing-styles
  name <CanvasPathDrawingStyles>
  take line-cap, name <lineCap>
    like canvas-line-cap
    lock free
  take line-dash-offset
    name <lineDashOffset>
    like native-number
    lock free
  take line-join, name <lineJoin>
    like canvas-line-join
    lock free
  take line-width, name <lineWidth>
    like native-number
    lock free
  take miter-limit, name <miterLimit>
    like native-number
    lock free

  task get-line-dash, name <getLineDash>
    free seed
      like list-link-text
        like native-number

  task set-line-dash, name <setLineDash>
    take segments
      like list-link-text
        like native-number
    free seed, like native-void
