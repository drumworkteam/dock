
load @drumwork/moon/code/javascript/base/native-array
  take form native-array

load @drumwork/moon/code/javascript/base/native-string
  take form native-string

load @drumwork/moon/code/javascript/base/native-void
  take form native-void

load @drumwork/moon/code/javascript/dom/node
  take form node

load @drumwork/seed/code/form/or
  take form or
    save form or-link-text

form child-node, name <ChildNode>
  task after, name <after>
    take nodes
      like native-array
        like or-link-text
          like node
          like native-string
      rest true
    free seed, like native-void

  task before, name <before>
    take nodes
      like native-array
        like or-link-text
          like node
          like native-string
      rest true
    free seed, like native-void

  task remove, name <remove>
    free seed, like native-void

  task replace-with, name <replaceWith>
    take nodes
      like native-array
        like or-link-text
          like node
          like native-string
      rest true
    free seed, like native-void
