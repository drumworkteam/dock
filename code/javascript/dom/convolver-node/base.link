
load @tunebond/moon
  take suit or

load @tunebond/bolt/code/javascript/native
  take face native-boolean
  take face native-null

load @tunebond/bolt/code/javascript/dom
  take face audio-buffer
  take face audio-node

face convolver-node, name <ConvolverNode>
  base audio-node

  link buffer, name <buffer>
    like or
      like audio-buffer
      like native-null
    flex true
  link normalize, name <normalize>
    like native-boolean
    flex true
