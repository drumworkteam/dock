
load @tunebond/nest/code/javascript/dom
  take face abort-signal
  take face credential-mediation-requirement
  take face public-key-credential-request-options

face credential-request-options, name <CredentialRequestOptions>
  link mediation, name <mediation>
    like credential-mediation-requirement
    flex true
  link public-key, name <publicKey>
    like public-key-credential-request-options
    flex true
  link signal, name <signal>
    like abort-signal
    flex true
