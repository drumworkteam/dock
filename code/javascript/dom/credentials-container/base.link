
load @treesurf/nest/code/javascript/base/native-null
  take form native-null

load @treesurf/nest/code/javascript/base/native-void
  take form native-void

load @treesurf/nest/code/javascript/dom/credential
  take form credential

load @treesurf/nest/code/javascript/dom/credential-creation-options
  take form credential-creation-options

load @treesurf/nest/code/javascript/dom/credential-request-options
  take form credential-request-options

load @treesurf/nest/code/javascript/es5/promise
  take form promise

load @treesurf/nest/code/form/or
  take form or
    save form or-link-text

form credentials-container, name <CredentialsContainer>
  task create, name <create>
    take options, like credential-creation-options
      void true
    free seed
      like promise
        like or-link-text
          like credential
          like native-null

  task get, name <get>
    take options, like credential-request-options
      void true
    free seed
      like promise
        like or-link-text
          like credential
          like native-null

  task prevent-silent-access, name <preventSilentAccess>
    free seed
      like promise
        like native-void

  task store, name <store>
    take credential, like credential
    free seed
      like promise
        like credential
