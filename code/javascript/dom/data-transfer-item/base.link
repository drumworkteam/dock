
load @drumwork/land/code/javascript/base/native-null
  take form native-null

load @drumwork/land/code/javascript/base/native-string
  take form native-string

load @drumwork/land/code/javascript/base/native-void
  take form native-void

load @drumwork/land/code/javascript/dom/file
  take form file

load @drumwork/land/code/javascript/dom/file-system-entry
  take form file-system-entry

load @drumwork/land/code/javascript/dom/function-string-callback
  take form function-string-callback

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form data-transfer-item
  name <DataTransferItem>
  take kind, name <kind>
    like native-string
  take type, name <type>
    like native-string

  task get-as-file, name <getAsFile>
    free seed
      like sink-link-text
        like file
        like native-null

  task get-as-string, name <getAsString>
    take callback
      like sink-link-text
        like function-string-callback
        like native-null
    free seed, like native-void

  task webkit-get-as-entry
    name <webkitGetAsEntry>
    free seed
      like sink-link-text
        like file-system-entry
        like native-null
