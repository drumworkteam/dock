
load @tunebond/moon
  take suit or

load @tunebond/nest/code/javascript/native
  take face native-number
  take face native-string
  take face native-void

load @tunebond/nest/code/javascript/dom
  take face data-transfer-item-list
  take face element
  take face file-list

load @tunebond/nest/code/javascript/es5
  take face readonly-array

face data-transfer, name <DataTransfer>
  link drop-effect, name <dropEffect>
    like or
      text <none>
      text <copy>
      text <link>
      text <move>
    flex true
  link effect-allowed, name <effectAllowed>
    like or
      text <none>
      text <copy>
      text <copyLink>
      text <copyMove>
      text <link>
      text <linkMove>
      text <move>
      text <all>
      text <uninitialized>
    flex true
  link files, name <files>
    like file-list
  link items, name <items>
    like data-transfer-item-list
  link types, name <types>
    like readonly-array
      like native-string

  task clear-data, name <clearData>
    take format, like native-string
      void true
    free seed, like native-void

  task get-data, name <getData>
    take format, like native-string
    free seed, like native-string

  task set-data, name <setData>
    take format, like native-string
    take data, like native-string
    free seed, like native-void

  task set-drag-image, name <setDragImage>
    take image, like element
    take x, like native-number
    take y, like native-number
    free seed, like native-void
