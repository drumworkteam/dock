
load @tunebond/nest/code/javascript/native
  take face native-number

load @tunebond/nest/code/javascript/dom
  take face device-motion-event-acceleration-init
  take face device-motion-event-rotation-rate-init
  take face event-init

face device-motion-event-init, name <DeviceMotionEventInit>
  base event-init

  link acceleration, name <acceleration>
    like device-motion-event-acceleration-init
    flex true
  link acceleration-including-gravity, name <accelerationIncludingGravity>
    like device-motion-event-acceleration-init
    flex true
  link interval, name <interval>
    like native-number
    flex true
  link rotation-rate, name <rotationRate>
    like device-motion-event-rotation-rate-init
    flex true
