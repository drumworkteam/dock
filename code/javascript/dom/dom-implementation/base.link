
load @tunebond/moon
  take suit any
  take suit or

load @tunebond/nest/code/javascript
  take face array

load @tunebond/nest/code/javascript/native
  take face native-null
  take face native-string

load @tunebond/nest/code/javascript/dom
  take face document
  take face document-type
  take face xml-document

face dom-implementation, name <DOMImplementation>
  task create-document, name <createDocument>
    take namespace
      like or
        like native-string
        like native-null
    take qualified-name
      like or
        like native-string
        like native-null
    take doctype
      like or
        like document-type
        like native-null
      void true
    free seed, like xml-document

  task create-document-type, name <createDocumentType>
    take qualified-name, like native-string
    take public-id, like native-string
    take system-id, like native-string
    free seed, like document-type

  task create-html-document, name <createHTMLDocument>
    take title, like native-string
      void true
    free seed, like document

  task has-feature, name <hasFeature>
    take args
      like array
        like any
      rest true
    free seed, text <true>
