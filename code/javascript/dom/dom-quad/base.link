
load @treesurf/moon
  take form any

load @treesurf/nest/code/javascript/dom
  take form dom-point
  take form dom-rect

form dom-quad, name <DOMQuad>
  link p1, name <p1>
    like dom-point
  link p2, name <p2>
    like dom-point
  link p3, name <p3>
    like dom-point
  link p4, name <p4>
    like dom-point

  task get-bounds, name <getBounds>
    like dom-rect

  task to-json, name <toJSON>
    like any
