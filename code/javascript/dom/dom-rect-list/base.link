
load @tunebond/moon
  take suit or

load @tunebond/nest/code/javascript/native
  take face native-null
  take face native-number

load @tunebond/nest/code/javascript/dom
  take face dom-rect

face dom-rect-list, name <DOMRectList>
  link length, name <length>
    like native-number

  task item, name <item>
    take index, like native-number
    free seed
      like or
        like dom-rect
        like native-null

  mesh index, like native-number
    like dom-rect
