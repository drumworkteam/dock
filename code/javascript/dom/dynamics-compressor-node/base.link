
load @treesurf/nest/code/javascript/native
  take face native-number

load @treesurf/nest/code/javascript/dom
  take face audio-node
  take face audio-param

face dynamics-compressor-node, name <DynamicsCompressorNode>
  base audio-node

  link attack, name <attack>
    like audio-param
  link knee, name <knee>
    like audio-param
  link ratio, name <ratio>
    like audio-param
  link reduction, name <reduction>
    like native-number
  link release, name <release>
    like audio-param
  link threshold, name <threshold>
    like audio-param
