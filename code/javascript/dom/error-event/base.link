
load @tunebond/moon
  take suit any

load @tunebond/bolt/code/javascript/native
  take face native-number
  take face native-string

load @tunebond/bolt/code/javascript/dom
  take face event

face error-event, name <ErrorEvent>
  base event

  link colno, name <colno>
    like native-number
  link error, name <error>
    like any
  link filename, name <filename>
    like native-string
  link lineno, name <lineno>
    like native-number
  link message, name <message>
    like native-string
