
load @tunebond/moon
  take suit any
  take suit index
  take suit key-list
  take suit like
  take suit or
  take suit task

load @tunebond/nest/code/javascript/native
  take face native-boolean
  take face native-null
  take face native-number
  take face native-string
  take face native-void

load @tunebond/nest/code/javascript/dom
  take face add-event-listener-options
  take face event
  take face event-listener-options
  take face event-listener-or-event-listener-object
  take face event-source-event-map
  take face event-target
  take face message-event

face event-source, name <EventSource>
  base event-target

  link onerror, name <onerror>
    like or
      like task
        take this, like event-source
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link onmessage, name <onmessage>
    like or
      like task
        take this, like event-source
        take ev, like message-event
        free seed, like any
      like native-null
    flex true
  link onopen, name <onopen>
    like or
      like task
        take this, like event-source
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link ready-state, name <readyState>
    like native-number
  link url, name <url>
    like native-string
  link with-credentials, name <withCredentials>
    like native-boolean
  link closed, name <CLOSED>
    like native-number
  link connecting, name <CONNECTING>
    like native-number
  link open, name <OPEN>
    like native-number

  task close, name <close>
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    head k
      base key-list
        like event-source-event-map

    take type, like k
    take listener
      like task
        take this, like event-source
        take ev
          like index
            like event-source-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    head k
      base key-list
        like event-source-event-map

    take type, like k
    take listener
      like task
        take this, like event-source
        take ev
          like index
            like event-source-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void
