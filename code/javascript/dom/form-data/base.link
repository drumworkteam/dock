
load @tunebond/moon
  take suit any
  take suit or
  take suit task

load @tunebond/bolt/code/javascript
  take face array

load @tunebond/bolt/code/javascript/native
  take face native-boolean
  take face native-null
  take face native-string
  take face native-void

load @tunebond/bolt/code/javascript/dom
  take face blob
  take face form-data-entry-value

face form-data, name <FormData>
  task append, name <append>
    take name, like native-string
    take value
      like or
        like native-string
        like blob
    take file-name, like native-string
      void true
    free seed, like native-void

  task delete, name <delete>
    take name, like native-string
    free seed, like native-void

  task get, name <get>
    take name, like native-string
    free seed
      like or
        like form-data-entry-value
        like native-null

  task get-all, name <getAll>
    take name, like native-string
    free seed
      like array
        like form-data-entry-value

  task has, name <has>
    take name, like native-string
    free seed, like native-boolean

  task set, name <set>
    take name, like native-string
    take value
      like or
        like native-string
        like blob
    take file-name, like native-string
      void true
    free seed, like native-void

  task for-each, name <forEach>
    take callbackfn
      like task
        take value, like form-data-entry-value
        take key, like native-string
        take parent, like form-data
        free seed, like native-void
    take this-arg, like any
      void true
    free seed, like native-void
