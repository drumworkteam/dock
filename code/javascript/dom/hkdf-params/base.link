
load @tunebond/nest/code/javascript/dom
  take face algorithm
  take face buffer-source
  take face hash-algorithm-identifier

face hkdf-params, name <HkdfParams>
  base algorithm

  link hash, name <hash>
    like hash-algorithm-identifier
    flex true
  link info, name <info>
    like buffer-source
    flex true
  link salt, name <salt>
    like buffer-source
    flex true
