
load @tunebond/moon
  take suit any
  take suit index
  take suit key-list
  take suit like
  take suit or
  take suit task

load @tunebond/bolt/code/javascript/native
  take face native-boolean
  take face native-null
  take face native-string
  take face native-void

load @tunebond/bolt/code/javascript/dom
  take face add-event-listener-options
  take face dom-exception
  take face dom-string-list
  take face event
  take face event-listener-options
  take face event-listener-or-event-listener-object
  take face event-target
  take face idb-database
  take face idb-object-store
  take face idb-transaction-event-map
  take face idb-transaction-mode

face idb-transaction, name <IDBTransaction>
  base event-target

  link db, name <db>
    like idb-database
  link error, name <error>
    like or
      like dom-exception
      like native-null
  link mode, name <mode>
    like idb-transaction-mode
  link object-store-names, name <objectStoreNames>
    like dom-string-list
  link onabort, name <onabort>
    like or
      like task
        take this, like idb-transaction
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link oncomplete, name <oncomplete>
    like or
      like task
        take this, like idb-transaction
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link onerror, name <onerror>
    like or
      like task
        take this, like idb-transaction
        take ev, like event
        free seed, like any
      like native-null
    flex true

  task abort, name <abort>
    free seed, like native-void

  task commit, name <commit>
    free seed, like native-void

  task object-store, name <objectStore>
    take name, like native-string
    free seed, like idb-object-store

  task add-event-listener, name <addEventListener>
    head k
      base key-list
        like idb-transaction-event-map

    take type, like k
    take listener
      like task
        take this, like idb-transaction
        take ev
          like index
            like idb-transaction-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    head k
      base key-list
        like idb-transaction-event-map

    take type, like k
    take listener
      like task
        take this, like idb-transaction
        take ev
          like index
            like idb-transaction-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void
