
load @tunebond/bolt/code/javascript
  take face array

load @tunebond/bolt/code/javascript/native
  take face native-number

load @tunebond/bolt/code/javascript/dom
  take face audio-node-options

face iir-filter-options, name <IIRFilterOptions>
  base audio-node-options

  link feedback, name <feedback>
    like array
      like native-number
    flex true
  link feedforward, name <feedforward>
    like array
      like native-number
    flex true
