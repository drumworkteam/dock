
load @drumwork/land/code/javascript/base/form/iterable
  take form iterable

load @drumwork/land/code/javascript/base/form/native-number
  take form native-number

load @drumwork/land/code/javascript/dom/base/form/base-audio-context
  take form base-audio-context

load @drumwork/land/code/javascript/dom/base/form/iir-filter-node
  take form iir-filter-node

load @drumwork/land/code/javascript/dom/base/form/periodic-wave
  take form periodic-wave

load @drumwork/land/code/javascript/dom/base/form/periodic-wave-constraints
  take form periodic-wave-constraints

form base-audio-context
  task create-iir-filter
    like task
      take feedforward
        like iterable
          like native-number
      take feedback
        like iterable
          like native-number
      free seed, like iir-filter-node

  task create-periodic-wave
    like task
      take real
        like iterable
          like native-number
      take imag
        like iterable
          like native-number
      take constraints, like periodic-wave-constraints
        void take
      free seed, like periodic-wave
