
load @tunebond/moon
  take suit or

load @tunebond/nest/code/javascript
  take face array

load @tunebond/nest/code/javascript/native
  take face iterable
  take face native-boolean

load @tunebond/nest/code/javascript/dom
  take face aes-cbc-params
  take face aes-ctr-params
  take face aes-derived-key-params
  take face aes-gcm-params
  take face aes-key-algorithm
  take face aes-key-gen-params
  take face algorithm-identifier
  take face buffer-source
  take face crypto-key
  take face crypto-key-pair
  take face ec-key-gen-params
  take face ec-key-import-params
  take face ecdh-key-derive-params
  take face hkdf-params
  take face hmac-import-params
  take face hmac-key-gen-params
  take face json-web-key
  take face key-format
  take face key-usage
  take face pbkdf2-params
  take face rsa-hashed-import-params
  take face rsa-hashed-key-gen-params
  take face rsa-oaep-params

load @tunebond/nest/code/javascript/es5
  take face exclude
  take face promise

load @tunebond/nest/code/javascript/dom/subtle-crypto
  take face subtle-crypto

face subtle-crypto, name <SubtleCrypto>
  task derive-key, name <deriveKey>
    take algorithm
      like or
        like algorithm-identifier
        like ecdh-key-derive-params
        like hkdf-params
        like pbkdf2-params
    take base-key, like crypto-key
    take derived-key-type
      like or
        like algorithm-identifier
        like aes-derived-key-params
        like hmac-import-params
        like hkdf-params
        like pbkdf2-params
    take extractable, like native-boolean
    take key-usages
      like iterable
        like key-usage
    free seed
      like promise
        like crypto-key

  task generate-key, name <generateKey>
    take algorithm
      like or
        like rsa-hashed-key-gen-params
        like ec-key-gen-params
    take extractable, like native-boolean
    take key-usages
      like array
        like key-usage
    free seed
      like promise
        like crypto-key-pair

  task generate-key, name <generateKey>
    take algorithm
      like or
        like aes-key-gen-params
        like hmac-key-gen-params
        like pbkdf2-params
    take extractable, like native-boolean
    take key-usages
      like array
        like key-usage
    free seed
      like promise
        like crypto-key

  task generate-key, name <generateKey>
    take algorithm, like algorithm-identifier
    take extractable, like native-boolean
    take key-usages
      like iterable
        like key-usage
    free seed
      like promise
        like or
          like crypto-key-pair
          like crypto-key

  task import-key, name <importKey>
    take format, text <jwk>
    take key-data, like json-web-key
    take algorithm
      like or
        like algorithm-identifier
        like rsa-hashed-import-params
        like ec-key-import-params
        like hmac-import-params
        like aes-key-algorithm
    take extractable, like native-boolean
    take key-usages
      like array
        like key-usage
    free seed
      like promise
        like crypto-key

  task import-key, name <importKey>
    take format
      like exclude
        like key-format
        text <jwk>
    take key-data, like buffer-source
    take algorithm
      like or
        like algorithm-identifier
        like rsa-hashed-import-params
        like ec-key-import-params
        like hmac-import-params
        like aes-key-algorithm
    take extractable, like native-boolean
    take key-usages
      like iterable
        like key-usage
    free seed
      like promise
        like crypto-key

  task unwrap-key, name <unwrapKey>
    take format, like key-format
    take wrapped-key, like buffer-source
    take unwrapping-key, like crypto-key
    take unwrap-algorithm
      like or
        like algorithm-identifier
        like rsa-oaep-params
        like aes-ctr-params
        like aes-cbc-params
        like aes-gcm-params
    take unwrapped-key-algorithm
      like or
        like algorithm-identifier
        like rsa-hashed-import-params
        like ec-key-import-params
        like hmac-import-params
        like aes-key-algorithm
    take extractable, like native-boolean
    take key-usages
      like iterable
        like key-usage
    free seed
      like promise
        like crypto-key
