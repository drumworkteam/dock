
load @treesurf/moon
  take form or

load @treesurf/nest/code/javascript
  take form array

load @treesurf/nest/code/javascript/native
  take form null
    save form native-null
  take form string
    save form native-string
  take form void
    save form native-void

load @treesurf/nest/code/javascript/dom
  take form animation-effect
  take form composite-operation
  take form computed-keyframe
  take form element
  take form iteration-composite-operation
  take form keyframe
  take form property-indexed-keyframes

form keyframe-effect, name <KeyframeEffect>
  base animation-effect

  link composite, name <composite>
    like composite-operation
    flex true
  link iteration-composite, name <iterationComposite>
    like iteration-composite-operation
    flex true
  link pseudo-element, name <pseudoElement>
    like or
      like native-string
      like native-null
    flex true
  link target, name <target>
    like or
      like element
      like native-null
    flex true

  task get-keyframes, name <getKeyframes>
    free seed
      like array
        like computed-keyframe

  task set-keyframes, name <setKeyframes>
    take keyframes
      like or
        like array
          like keyframe
        like property-indexed-keyframes
        like native-null
    free seed, like native-void
