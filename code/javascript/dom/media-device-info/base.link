
load @tunebond/moon
  take suit any

load @tunebond/bolt/code/javascript/native
  take face native-string

load @tunebond/bolt/code/javascript/dom
  take face media-device-kind

face media-device-info, name <MediaDeviceInfo>
  link device-id, name <deviceId>
    like native-string
  link group-id, name <groupId>
    like native-string
  link kind, name <kind>
    like media-device-kind
  link label, name <label>
    like native-string

  task to-json, name <toJSON>
    free seed, like any
