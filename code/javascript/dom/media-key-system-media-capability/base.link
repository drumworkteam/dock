
load @tunebond/moon
  take suit or

load @tunebond/nest/code/javascript/native
  take face native-null
  take face native-string

face media-key-system-media-capability, name <MediaKeySystemMediaCapability>
  link content-type, name <contentType>
    like native-string
    flex true
  link encryption-scheme, name <encryptionScheme>
    like or
      like native-string
      like native-null
    flex true
  link robustness, name <robustness>
    like native-string
    flex true
