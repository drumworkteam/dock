
load @treesurf/moon
  take face any
  take face index
  take face key-list
  take face like
  take face or
  take face task

load @treesurf/nest/code/javascript/native
  take face native-boolean
  take face native-null
  take face native-number
  take face native-string
  take face native-void

load @treesurf/nest/code/javascript/dom
  take face add-event-listener-options
  take face blob-event
  take face event
  take face event-listener-options
  take face event-listener-or-event-listener-object
  take face event-target
  take face media-recorder-event-map
  take face media-stream
  take face recording-state

face media-recorder, name <MediaRecorder>
  base event-target

  link audio-bits-per-second, name <audioBitsPerSecond>
    like native-number
  link mime-type, name <mimeType>
    like native-string
  link ondataavailable, name <ondataavailable>
    like or
      like task
        take this, like media-recorder
        take ev, like blob-event
        free seed, like any
      like native-null
    flex true
  link onerror, name <onerror>
    like or
      like task
        take this, like media-recorder
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link onpause, name <onpause>
    like or
      like task
        take this, like media-recorder
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link onresume, name <onresume>
    like or
      like task
        take this, like media-recorder
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link onstart, name <onstart>
    like or
      like task
        take this, like media-recorder
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link onstop, name <onstop>
    like or
      like task
        take this, like media-recorder
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link state, name <state>
    like recording-state
  link stream, name <stream>
    like media-stream
  link video-bits-per-second, name <videoBitsPerSecond>
    like native-number

  task pause, name <pause>
    free seed, like native-void

  task request-data, name <requestData>
    free seed, like native-void

  task resume, name <resume>
    free seed, like native-void

  task start, name <start>
    take timeslice, like native-number
      void true
    free seed, like native-void

  task stop, name <stop>
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    head k
      base key-list
        like media-recorder-event-map

    take type, like k
    take listener
      like task
        take this, like media-recorder
        take ev
          like index
            like media-recorder-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    head k
      base key-list
        like media-recorder-event-map

    take type, like k
    take listener
      like task
        take this, like media-recorder
        take ev
          like index
            like media-recorder-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void
