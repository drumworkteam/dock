
load @treesurf/moon
  take face or

load @treesurf/nest/code/javascript/native
  take face native-null
  take face native-void

load @treesurf/nest/code/javascript/dom
  take face media-metadata
  take face media-position-state
  take face media-session-action
  take face media-session-action-handler
  take face media-session-playback-state

face media-session, name <MediaSession>
  link metadata, name <metadata>
    like or
      like media-metadata
      like native-null
    flex true
  link playback-state, name <playbackState>
    like media-session-playback-state
    flex true

  task set-action-handler, name <setActionHandler>
    take action, like media-session-action
    take handler
      like or
        like media-session-action-handler
        like native-null
    free seed, like native-void

  task set-position-state, name <setPositionState>
    take state, like media-position-state
      void true
    free seed, like native-void
