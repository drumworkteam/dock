
load @treesurf/moon
  take suit any
  take suit index
  take suit key-list
  take suit like
  take suit or
  take suit task

load @treesurf/nest/code/javascript/native
  take face native-boolean
  take face native-null
  take face native-number
  take face native-string
  take face native-void

load @treesurf/nest/code/javascript/dom
  take face add-event-listener-options
  take face end-of-stream-error
  take face event
  take face event-listener-options
  take face event-listener-or-event-listener-object
  take face event-target
  take face media-source-event-map
  take face ready-state
  take face source-buffer
  take face source-buffer-list

face media-source, name <MediaSource>
  base event-target

  link active-source-buffers, name <activeSourceBuffers>
    like source-buffer-list
  link duration, name <duration>
    like native-number
    flex true
  link onsourceclose, name <onsourceclose>
    like or
      like task
        take this, like media-source
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link onsourceended, name <onsourceended>
    like or
      like task
        take this, like media-source
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link onsourceopen, name <onsourceopen>
    like or
      like task
        take this, like media-source
        take ev, like event
        free seed, like any
      like native-null
    flex true
  link ready-state, name <readyState>
    like ready-state
  link source-buffers, name <sourceBuffers>
    like source-buffer-list

  task add-source-buffer, name <addSourceBuffer>
    take type, like native-string
    free seed, like source-buffer

  task clear-live-seekable-range, name <clearLiveSeekableRange>
    free seed, like native-void

  task end-of-stream, name <endOfStream>
    take error, like end-of-stream-error
      void true
    free seed, like native-void

  task remove-source-buffer, name <removeSourceBuffer>
    take source-buffer, like source-buffer
    free seed, like native-void

  task set-live-seekable-range, name <setLiveSeekableRange>
    take start, like native-number
    take end, like native-number
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    head k
      base key-list
        like media-source-event-map

    take type, like k
    take listener
      like task
        take this, like media-source
        take ev
          like index
            like media-source-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    head k
      base key-list
        like media-source-event-map

    take type, like k
    take listener
      like task
        take this, like media-source
        take ev
          like index
            like media-source-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void
