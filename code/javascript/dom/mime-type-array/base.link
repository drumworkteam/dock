
load @tunebond/moon
  take suit any

load @tunebond/nest/code/javascript/native
  take face native-number
  take face native-string

face mime-type-array, name <MimeTypeArray>
  link length, name <length>
    like native-number

  task item, name <item>
    take index, like native-number
    free seed, like any

  task named-item, name <namedItem>
    take name, like native-string
    free seed, like any

  mesh index, like native-number
    like any
