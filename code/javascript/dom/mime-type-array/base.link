
load @treesurf/moon
  take form any

load @treesurf/nest/code/javascript/native
  take form native-number
  take form native-string

form mime-type-array, name <MimeTypeArray>
  link length, name <length>
    like native-number

  task item, name <item>
    take index, like native-number
    free seed, like any

  task named-item, name <namedItem>
    take name, like native-string
    free seed, like any

  mesh index, like native-number
    like any
