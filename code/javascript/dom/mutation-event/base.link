
load @tunebond/moon
  take suit or

load @tunebond/nest/code/javascript/native
  take face native-boolean
  take face native-null
  take face native-number
  take face native-string
  take face native-void

load @tunebond/nest/code/javascript/dom
  take face event
  take face node

face mutation-event, name <MutationEvent>
  base event

  link attr-change, name <attrChange>
    like native-number
  link attr-name, name <attrName>
    like native-string
  link new-value, name <newValue>
    like native-string
  link prev-value, name <prevValue>
    like native-string
  link related-node, name <relatedNode>
    like or
      like node
      like native-null
  link addition, name <ADDITION>
    like native-number
  link modification, name <MODIFICATION>
    like native-number
  link removal, name <REMOVAL>
    like native-number

  task init-mutation-event, name <initMutationEvent>
    take type-arg, like native-string
    take bubbles-arg, like native-boolean
      void true
    take cancelable-arg, like native-boolean
      void true
    take related-node-arg
      like or
        like node
        like native-null
      void true
    take prev-value-arg, like native-string
      void true
    take new-value-arg, like native-string
      void true
    take attr-name-arg, like native-string
      void true
    take attr-change-arg, like native-number
      void true
    free seed, like native-void
