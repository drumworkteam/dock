
load @treesurf/nest/code/javascript/base/native-array
  take form native-array

load @treesurf/nest/code/javascript/base/native-void
  take form native-void

load @treesurf/nest/code/javascript/dom/mutation-observer-init
  take form mutation-observer-init

load @treesurf/nest/code/javascript/dom/mutation-record
  take form mutation-record

load @treesurf/nest/code/javascript/dom/node
  take form node

form mutation-observer, name <MutationObserver>
  task disconnect, name <disconnect>
    free seed, like native-void

  task observe, name <observe>
    take target, like node
    take options, like mutation-observer-init
      void true
    free seed, like native-void

  task take-records, name <takeRecords>
    free seed
      like native-array
        like mutation-record
