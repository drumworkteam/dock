
load @treesurf/moon
  take suit or

load @treesurf/nest/code/javascript/native
  take face native-null
  take face native-string

load @treesurf/nest/code/javascript/dom
  take face mutation-record-type
  take face node
  take face node-list

face mutation-record, name <MutationRecord>
  link added-nodes, name <addedNodes>
    like node-list
  link attribute-name, name <attributeName>
    like or
      like native-string
      like native-null
  link attribute-namespace, name <attributeNamespace>
    like or
      like native-string
      like native-null
  link next-sibling, name <nextSibling>
    like or
      like node
      like native-null
  link old-value, name <oldValue>
    like or
      like native-string
      like native-null
  link previous-sibling, name <previousSibling>
    like or
      like node
      like native-null
  link removed-nodes, name <removedNodes>
    like node-list
  link target, name <target>
    like node
  link type, name <type>
    like mutation-record-type
