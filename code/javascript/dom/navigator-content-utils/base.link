
load @tunebond/moon
  take suit or

load @tunebond/bolt/code/javascript/native
  take face native-string
  take face native-void

load @tunebond/bolt/code/javascript/dom
  take face url

face navigator-content-utils, name <NavigatorContentUtils>
  task register-protocol-handler, name <registerProtocolHandler>
    take scheme, like native-string
    take url
      like or
        like native-string
        like url
    free seed, like native-void
