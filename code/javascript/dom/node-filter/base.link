
load @treesurf/moon
  take face form
  take face or
  take face task

load @treesurf/nest/code/javascript/native
  take face native-number

load @treesurf/nest/code/javascript/dom
  take face node

face node-filter, name <NodeFilter>
  like or
    like task
      take node, like node
      free seed, like native-number
    like form
      task accept-node, name <acceptNode>
        take node, like node
        free seed, like native-number
