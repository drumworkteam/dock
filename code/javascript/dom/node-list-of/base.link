
load @treesurf/moon
  take form any
  take form like
  take form task

load @treesurf/nest/code/javascript/native
  take form native-number
  take form native-void

load @treesurf/nest/code/javascript/dom
  take form node
  take form node-list

form node-list-of, name <NodeListOf>
  base node-list

  head t-node
    base node

  task item, name <item>
    take index, like native-number
    free seed, like t-node

  task for-each, name <forEach>
    take callbackfn
      like task
        take value, like t-node
        take key, like native-number
        take parent
          like node-list-of
            like t-node
        free seed, like native-void
    take this-arg, like any
      void true
    free seed, like native-void

  mesh index, like native-number
    like t-node
