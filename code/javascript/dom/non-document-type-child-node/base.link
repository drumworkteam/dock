
load @drumwork/land/code/javascript/base/native-null
  take form native-null

load @drumwork/land/code/javascript/dom/element
  take form element

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form non-document-type-child-node
  name <NonDocumentTypeChildNode>
  take next-element-sibling
    name <nextElementSibling>
    like sink-link-text
      like element
      like native-null
  take previous-element-sibling
    name <previousElementSibling>
    like sink-link-text
      like element
      like native-null
