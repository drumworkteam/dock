
load @treesurf/moon
  take suit or

load @treesurf/nest/code/javascript/native
  take face native-null
  take face native-void

load @treesurf/nest/code/javascript/dom
  take face g-lenum
  take face g-lint
  take face g-lsizei
  take face web-gl-texture

face ovr-multiview2, name <OVR_multiview2>
  link framebuffer-attachment-texture-base-view-index-ovr
    name <FRAMEBUFFER_ATTACHMENT_TEXTURE_BASE_VIEW_INDEX_OVR>
    like g-lenum
  link framebuffer-attachment-texture-num-views-ovr
    name <FRAMEBUFFER_ATTACHMENT_TEXTURE_NUM_VIEWS_OVR>
    like g-lenum
  link framebuffer-incomplete-view-targets-ovr, name <FRAMEBUFFER_INCOMPLETE_VIEW_TARGETS_OVR>
    like g-lenum
  link max-views-ovr, name <MAX_VIEWS_OVR>
    like g-lenum

  task framebuffer-texture-multiview-ovr, name <framebufferTextureMultiviewOVR>
    take target, like g-lenum
    take attachment, like g-lenum
    take texture
      like or
        like web-gl-texture
        like native-null
    take level, like g-lint
    take base-view-index, like g-lint
    take num-views, like g-lsizei
    free seed, like native-void
