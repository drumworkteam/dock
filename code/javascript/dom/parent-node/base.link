
load @tunebond/moon
  take suit index
  take suit key-list
  take suit like
  take suit or

load @tunebond/nest/code/javascript
  take face array

load @tunebond/nest/code/javascript/native
  take face native-null
  take face native-number
  take face native-string
  take face native-void

load @tunebond/nest/code/javascript/dom
  take face element
  take face html-collection
  take face html-element-tag-name-map
  take face node
  take face node-list-of
  take face svg-element-tag-name-map

face parent-node, name <ParentNode>
  link child-element-count, name <childElementCount>
    like native-number
  link children, name <children>
    like html-collection
  link first-element-child, name <firstElementChild>
    like or
      like element
      like native-null
  link last-element-child, name <lastElementChild>
    like or
      like element
      like native-null

  task append, name <append>
    take nodes
      like array
        like or
          like node
          like native-string
      rest true
    free seed, like native-void

  task prepend, name <prepend>
    take nodes
      like array
        like or
          like node
          like native-string
      rest true
    free seed, like native-void

  task query-selector, name <querySelector>
    head k
      base key-list
        like html-element-tag-name-map

    take selectors, like k
    free seed
      like or
        like index
          like html-element-tag-name-map
          like k
        like native-null

  task query-selector, name <querySelector>
    head k
      base key-list
        like svg-element-tag-name-map

    take selectors, like k
    free seed
      like or
        like index
          like svg-element-tag-name-map
          like k
        like native-null

  task query-selector, name <querySelector>
    head e
      base element
      back element

    take selectors, like native-string
    free seed
      like or
        like e
        like native-null

  task query-selector-all, name <querySelectorAll>
    head k
      base key-list
        like html-element-tag-name-map

    take selectors, like k
    free seed
      like node-list-of
        like index
          like html-element-tag-name-map
          like k

  task query-selector-all, name <querySelectorAll>
    head k
      base key-list
        like svg-element-tag-name-map

    take selectors, like k
    free seed
      like node-list-of
        like index
          like svg-element-tag-name-map
          like k

  task query-selector-all, name <querySelectorAll>
    head e
      base element
      back element

    take selectors, like native-string
    free seed
      like node-list-of
        like e

  task replace-children, name <replaceChildren>
    take nodes
      like array
        like or
          like node
          like native-string
      rest true
    free seed, like native-void
