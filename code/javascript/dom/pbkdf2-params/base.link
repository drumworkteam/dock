
load @tunebond/nest/code/javascript/native
  take face native-number

load @tunebond/nest/code/javascript/dom
  take face algorithm
  take face buffer-source
  take face hash-algorithm-identifier

face pbkdf2-params, name <Pbkdf2Params>
  base algorithm

  link hash, name <hash>
    like hash-algorithm-identifier
    flex true
  link iterations, name <iterations>
    like native-number
    flex true
  link salt, name <salt>
    like buffer-source
    flex true
