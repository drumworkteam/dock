
load @tunebond/nest/code/javascript/native
  take face native-string

load @tunebond/nest/code/javascript/dom
  take face character-data
  take face document
  take face link-style

face processing-instruction, name <ProcessingInstruction>
  base character-data
  base link-style

  link owner-document, name <ownerDocument>
    like document
  link target, name <target>
    like native-string
