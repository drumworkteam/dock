
load @treesurf/nest/code/javascript
  take face array

load @treesurf/nest/code/javascript/native
  take face native-number

load @treesurf/nest/code/javascript/dom
  take face attestation-conveyance-preference
  take face authentication-extensions-client-inputs
  take face authenticator-selection-criteria
  take face buffer-source
  take face public-key-credential-descriptor
  take face public-key-credential-parameters
  take face public-key-credential-rp-entity
  take face public-key-credential-user-entity

face public-key-credential-creation-options, name <PublicKeyCredentialCreationOptions>
  link attestation, name <attestation>
    like attestation-conveyance-preference
    flex true
  link authenticator-selection, name <authenticatorSelection>
    like authenticator-selection-criteria
    flex true
  link challenge, name <challenge>
    like buffer-source
    flex true
  link exclude-credentials, name <excludeCredentials>
    like array
      like public-key-credential-descriptor
    flex true
  link extensions, name <extensions>
    like authentication-extensions-client-inputs
    flex true
  link pub-key-cred-params, name <pubKeyCredParams>
    like array
      like public-key-credential-parameters
    flex true
  link rp, name <rp>
    like public-key-credential-rp-entity
    flex true
  link timeout, name <timeout>
    like native-number
    flex true
  link user, name <user>
    like public-key-credential-user-entity
    flex true
