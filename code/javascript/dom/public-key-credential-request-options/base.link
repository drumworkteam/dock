
load @drumwork/land/code/javascript/base/native-number
  take form native-number

load @drumwork/land/code/javascript/base/native-string
  take form native-string

load @drumwork/land/code/javascript/dom/authentication-extensions-client-inputs
  take form authentication-extensions-client-inputs

load @drumwork/land/code/javascript/dom/buffer-source
  take form buffer-source

load @drumwork/land/code/javascript/dom/public-key-credential-descriptor
  take form public-key-credential-descriptor

load @drumwork/land/code/javascript/dom/user-verification-requirement
  take form user-verification-requirement

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

form public-key-credential-request-options
  name <PublicKeyCredentialRequestOptions>
  take allow-credentials
    name <allowCredentials>
    like list-link-text
      like public-key-credential-descriptor
    lock free
  take challenge, name <challenge>
    like buffer-source
    lock free
  take extensions, name <extensions>
    like authentication-extensions-client-inputs
    lock free
  take rp-id, name <rpId>
    like native-string
    lock free
  take timeout, name <timeout>
    like native-number
    lock free
  take user-verification
    name <userVerification>
    like user-verification-requirement
    lock free
