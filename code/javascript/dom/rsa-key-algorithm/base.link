
load @tunebond/bolt/code/javascript/native
  take face native-number

load @tunebond/bolt/code/javascript/dom
  take face big-integer
  take face key-algorithm

face rsa-key-algorithm, name <RsaKeyAlgorithm>
  base key-algorithm

  link modulus-length, name <modulusLength>
    like native-number
    flex true
  link public-exponent, name <publicExponent>
    like big-integer
    flex true
