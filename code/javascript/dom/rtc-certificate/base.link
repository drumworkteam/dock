
load @tunebond/bolt/code/javascript
  take face array

load @tunebond/bolt/code/javascript/dom
  take face dom-time-stamp
  take face rtc-dtls-fingerprint

face rtc-certificate, name <RTCCertificate>
  link expires, name <expires>
    like dom-time-stamp

  task get-fingerprints, name <getFingerprints>
    free seed
      like array
        like rtc-dtls-fingerprint
