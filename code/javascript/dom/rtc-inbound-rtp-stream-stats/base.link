
load @tunebond/nest/code/javascript/native
  take face native-number
  take face native-string

load @tunebond/nest/code/javascript/dom
  take face rtc-received-rtp-stream-stats

face rtc-inbound-rtp-stream-stats, name <RTCInboundRtpStreamStats>
  base rtc-received-rtp-stream-stats

  link fir-count, name <firCount>
    like native-number
    flex true
  link frames-decoded, name <framesDecoded>
    like native-number
    flex true
  link nack-count, name <nackCount>
    like native-number
    flex true
  link pli-count, name <pliCount>
    like native-number
    flex true
  link qp-sum, name <qpSum>
    like native-number
    flex true
  link remote-id, name <remoteId>
    like native-string
    flex true
