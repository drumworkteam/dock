
load @treesurf/nest/code/javascript
  take face array

load @treesurf/nest/code/javascript/dom
  take face rtc-rtp-codec-capability
  take face rtc-rtp-header-extension-capability

face rtc-rtp-capabilities, name <RTCRtpCapabilities>
  link codecs, name <codecs>
    like array
      like rtc-rtp-codec-capability
    flex true
  link header-extensions, name <headerExtensions>
    like array
      like rtc-rtp-header-extension-capability
    flex true
