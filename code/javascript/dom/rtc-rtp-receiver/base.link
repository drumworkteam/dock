
load @drumwork/land/code/javascript/base/native-null
  take form native-null

load @drumwork/land/code/javascript/dom/media-stream-track
  take form media-stream-track

load @drumwork/land/code/javascript/dom/rtc-dtls-transport
  take form rtc-dtls-transport

load @drumwork/land/code/javascript/dom/rtc-rtp-contributing-source
  take form rtc-rtp-contributing-source

load @drumwork/land/code/javascript/dom/rtc-rtp-receive-parameters
  take form rtc-rtp-receive-parameters

load @drumwork/land/code/javascript/dom/rtc-rtp-synchronization-source
  take form rtc-rtp-synchronization-source

load @drumwork/land/code/javascript/dom/rtc-stats-report
  take form rtc-stats-report

load @drumwork/land/code/javascript/es5/promise
  take form promise

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form rtc-rtp-receiver
  name <RTCRtpReceiver>
  take track, name <track>
    like media-stream-track
  take transport, name <transport>
    like sink-link-text
      like rtc-dtls-transport
      like native-null

  task get-contributing-sources
    name <getContributingSources>
    free seed
      like list-link-text
        like rtc-rtp-contributing-source

  task get-parameters
    name <getParameters>
    free seed, like rtc-rtp-receive-parameters

  task get-stats, name <getStats>
    free seed
      like promise
        like rtc-stats-report

  task get-synchronization-sources
    name <getSynchronizationSources>
    free seed
      like list-link-text
        like rtc-rtp-synchronization-source
