
load @treesurf/moon
  take suit or

load @treesurf/nest/code/javascript/native
  take face native-null
  take face native-string
  take face native-void

load @treesurf/nest/code/javascript/dom
  take face rtc-rtp-receiver
  take face rtc-rtp-sender
  take face rtc-rtp-transceiver-direction

face rtc-rtp-transceiver, name <RTCRtpTransceiver>
  link current-direction, name <currentDirection>
    like or
      like rtc-rtp-transceiver-direction
      like native-null
  link direction, name <direction>
    like rtc-rtp-transceiver-direction
    flex true
  link mid, name <mid>
    like or
      like native-string
      like native-null
  link receiver, name <receiver>
    like rtc-rtp-receiver
  link sender, name <sender>
    like rtc-rtp-sender

  task stop, name <stop>
    free seed, like native-void
