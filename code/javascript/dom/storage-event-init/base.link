
load @tunebond/moon
  take suit or

load @tunebond/bolt/code/javascript/native
  take face native-null
  take face native-string

load @tunebond/bolt/code/javascript/dom
  take face event-init
  take face storage

face storage-event-init, name <StorageEventInit>
  base event-init

  link key, name <key>
    like or
      like native-string
      like native-null
    flex true
  link new-value, name <newValue>
    like or
      like native-string
      like native-null
    flex true
  link old-value, name <oldValue>
    like or
      like native-string
      like native-null
    flex true
  link storage-area, name <storageArea>
    like or
      like storage
      like native-null
    flex true
  link url, name <url>
    like native-string
    flex true
