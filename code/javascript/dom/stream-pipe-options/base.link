
load @treesurf/nest/code/javascript/native
  take face native-boolean

load @treesurf/nest/code/javascript/dom
  take face abort-signal

face stream-pipe-options, name <StreamPipeOptions>
  link prevent-abort, name <preventAbort>
    like native-boolean
    flex true
  link prevent-cancel, name <preventCancel>
    like native-boolean
    flex true
  link prevent-close, name <preventClose>
    like native-boolean
    flex true
  link signal, name <signal>
    like abort-signal
    flex true
