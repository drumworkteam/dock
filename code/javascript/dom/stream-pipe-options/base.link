
load @drumwork/land/code/javascript/base/native-boolean
  take form native-boolean

load @drumwork/land/code/javascript/dom/abort-signal
  take form abort-signal

form stream-pipe-options
  name <StreamPipeOptions>
  take prevent-abort
    name <preventAbort>
    like native-boolean
    lock free
  take prevent-cancel
    name <preventCancel>
    like native-boolean
    lock free
  take prevent-close
    name <preventClose>
    like native-boolean
    lock free
  take signal, name <signal>
    like abort-signal
    lock free
