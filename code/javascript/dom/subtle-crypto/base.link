
load @tunebond/moon
  take suit any
  take suit or

load @tunebond/nest/code/javascript
  take face array

load @tunebond/nest/code/javascript/native
  take face native-boolean
  take face native-number

load @tunebond/nest/code/javascript/dom
  take face aes-cbc-params
  take face aes-ctr-params
  take face aes-derived-key-params
  take face aes-gcm-params
  take face aes-key-algorithm
  take face aes-key-gen-params
  take face algorithm-identifier
  take face buffer-source
  take face crypto-key
  take face crypto-key-pair
  take face ec-key-gen-params
  take face ec-key-import-params
  take face ecdh-key-derive-params
  take face ecdsa-params
  take face hkdf-params
  take face hmac-import-params
  take face hmac-key-gen-params
  take face json-web-key
  take face key-format
  take face key-usage
  take face pbkdf2-params
  take face rsa-hashed-import-params
  take face rsa-hashed-key-gen-params
  take face rsa-oaep-params
  take face rsa-pss-params

load @tunebond/nest/code/javascript/es5
  take face array-buffer
  take face exclude
  take face promise

face subtle-crypto, name <SubtleCrypto>
  task decrypt, name <decrypt>
    take algorithm
      like or
        like algorithm-identifier
        like rsa-oaep-params
        like aes-ctr-params
        like aes-cbc-params
        like aes-gcm-params
    take key, like crypto-key
    take data, like buffer-source
    free seed
      like promise
        like any

  task derive-bits, name <deriveBits>
    take algorithm
      like or
        like algorithm-identifier
        like ecdh-key-derive-params
        like hkdf-params
        like pbkdf2-params
    take base-key, like crypto-key
    take length, like native-number
    free seed
      like promise
        like array-buffer

  task derive-key, name <deriveKey>
    take algorithm
      like or
        like algorithm-identifier
        like ecdh-key-derive-params
        like hkdf-params
        like pbkdf2-params
    take base-key, like crypto-key
    take derived-key-type
      like or
        like algorithm-identifier
        like aes-derived-key-params
        like hmac-import-params
        like hkdf-params
        like pbkdf2-params
    take extractable, like native-boolean
    take key-usages
      like array
        like key-usage
    free seed
      like promise
        like crypto-key

  task digest, name <digest>
    take algorithm, like algorithm-identifier
    take data, like buffer-source
    free seed
      like promise
        like array-buffer

  task encrypt, name <encrypt>
    take algorithm
      like or
        like algorithm-identifier
        like rsa-oaep-params
        like aes-ctr-params
        like aes-cbc-params
        like aes-gcm-params
    take key, like crypto-key
    take data, like buffer-source
    free seed
      like promise
        like any

  task export-key, name <exportKey>
    take format, text <jwk>
    take key, like crypto-key
    free seed
      like promise
        like json-web-key

  task export-key, name <exportKey>
    take format
      like exclude
        like key-format
        text <jwk>
    take key, like crypto-key
    free seed
      like promise
        like array-buffer

  task generate-key, name <generateKey>
    take algorithm
      like or
        like rsa-hashed-key-gen-params
        like ec-key-gen-params
    take extractable, like native-boolean
    take key-usages
      like array
        like key-usage
    free seed
      like promise
        like crypto-key-pair

  task generate-key, name <generateKey>
    take algorithm
      like or
        like aes-key-gen-params
        like hmac-key-gen-params
        like pbkdf2-params
    take extractable, like native-boolean
    take key-usages
      like array
        like key-usage
    free seed
      like promise
        like crypto-key

  task generate-key, name <generateKey>
    take algorithm, like algorithm-identifier
    take extractable, like native-boolean
    take key-usages
      like array
        like key-usage
    free seed
      like promise
        like or
          like crypto-key-pair
          like crypto-key

  task import-key, name <importKey>
    take format, text <jwk>
    take key-data, like json-web-key
    take algorithm
      like or
        like algorithm-identifier
        like rsa-hashed-import-params
        like ec-key-import-params
        like hmac-import-params
        like aes-key-algorithm
    take extractable, like native-boolean
    take key-usages
      like array
        like key-usage
    free seed
      like promise
        like crypto-key

  task import-key, name <importKey>
    take format
      like exclude
        like key-format
        text <jwk>
    take key-data, like buffer-source
    take algorithm
      like or
        like algorithm-identifier
        like rsa-hashed-import-params
        like ec-key-import-params
        like hmac-import-params
        like aes-key-algorithm
    take extractable, like native-boolean
    take key-usages
      like array
        like key-usage
    free seed
      like promise
        like crypto-key

  task sign, name <sign>
    take algorithm
      like or
        like algorithm-identifier
        like rsa-pss-params
        like ecdsa-params
    take key, like crypto-key
    take data, like buffer-source
    free seed
      like promise
        like array-buffer

  task unwrap-key, name <unwrapKey>
    take format, like key-format
    take wrapped-key, like buffer-source
    take unwrapping-key, like crypto-key
    take unwrap-algorithm
      like or
        like algorithm-identifier
        like rsa-oaep-params
        like aes-ctr-params
        like aes-cbc-params
        like aes-gcm-params
    take unwrapped-key-algorithm
      like or
        like algorithm-identifier
        like rsa-hashed-import-params
        like ec-key-import-params
        like hmac-import-params
        like aes-key-algorithm
    take extractable, like native-boolean
    take key-usages
      like array
        like key-usage
    free seed
      like promise
        like crypto-key

  task verify, name <verify>
    take algorithm
      like or
        like algorithm-identifier
        like rsa-pss-params
        like ecdsa-params
    take key, like crypto-key
    take signature, like buffer-source
    take data, like buffer-source
    free seed
      like promise
        like native-boolean

  task wrap-key, name <wrapKey>
    take format, like key-format
    take key, like crypto-key
    take wrapping-key, like crypto-key
    take wrap-algorithm
      like or
        like algorithm-identifier
        like rsa-oaep-params
        like aes-ctr-params
        like aes-cbc-params
        like aes-gcm-params
    free seed
      like promise
        like array-buffer
