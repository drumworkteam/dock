
load @tunebond/moon
  take suit any
  take suit index
  take suit key-list
  take suit like
  take suit or
  take suit task

load @tunebond/nest/code/javascript/native
  take face native-boolean
  take face native-null
  take face native-string
  take face native-void

load @tunebond/nest/code/javascript/dom
  take face add-event-listener-options
  take face document-and-element-event-handlers
  take face element
  take face element-css-inline-style
  take face event-listener-options
  take face event-listener-or-event-listener-object
  take face global-event-handlers
  take face html-or-svg-element
  take face svg-element-event-map
  take face svgsvg-element

face svg-element, name <SVGElement>
  base element
  base document-and-element-event-handlers
  base document-and-element-event-handlers
  base element-css-inline-style
  base global-event-handlers
  base global-event-handlers
  base html-or-svg-element

  link class-name, name <className>
    like any
  link owner-svg-element, name <ownerSVGElement>
    like or
      like svgsvg-element
      like native-null
  link viewport-element, name <viewportElement>
    like or
      like svg-element
      like native-null

  task add-event-listener, name <addEventListener>
    head k
      base key-list
        like svg-element-event-map

    take type, like k
    take listener
      like task
        take this, like svg-element
        take ev
          like index
            like svg-element-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    head k
      base key-list
        like svg-element-event-map

    take type, like k
    take listener
      like task
        take this, like svg-element
        take ev
          like index
            like svg-element-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void
