
load @treesurf/moon
  take suit any
  take suit index
  take suit key-list
  take suit like
  take suit or
  take suit task

load @treesurf/nest/code/javascript/native
  take face native-boolean
  take face native-number
  take face native-string
  take face native-void

load @treesurf/nest/code/javascript/dom
  take face add-event-listener-options
  take face dom-point
  take face dom-point-init
  take face dom-rect
  take face event-listener-options
  take face event-listener-or-event-listener-object
  take face svg-animated-enumeration
  take face svg-animated-length
  take face svg-element-event-map
  take face svg-graphics-element

face svg-text-content-element, name <SVGTextContentElement>
  base svg-graphics-element

  link length-adjust, name <lengthAdjust>
    like svg-animated-enumeration
  link text-length, name <textLength>
    like svg-animated-length
  link lengthadjust-spacing, name <LENGTHADJUST_SPACING>
    like native-number
  link lengthadjust-spacingandglyphs, name <LENGTHADJUST_SPACINGANDGLYPHS>
    like native-number
  link lengthadjust-unknown, name <LENGTHADJUST_UNKNOWN>
    like native-number

  task get-char-num-at-position, name <getCharNumAtPosition>
    take point, like dom-point-init
      void true
    free seed, like native-number

  task get-computed-text-length, name <getComputedTextLength>
    free seed, like native-number

  task get-end-position-of-char, name <getEndPositionOfChar>
    take charnum, like native-number
    free seed, like dom-point

  task get-extent-of-char, name <getExtentOfChar>
    take charnum, like native-number
    free seed, like dom-rect

  task get-number-of-chars, name <getNumberOfChars>
    free seed, like native-number

  task get-rotation-of-char, name <getRotationOfChar>
    take charnum, like native-number
    free seed, like native-number

  task get-start-position-of-char, name <getStartPositionOfChar>
    take charnum, like native-number
    free seed, like dom-point

  task get-sub-string-length, name <getSubStringLength>
    take charnum, like native-number
    take nchars, like native-number
    free seed, like native-number

  task select-sub-string, name <selectSubString>
    take charnum, like native-number
    take nchars, like native-number
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    head k
      base key-list
        like svg-element-event-map

    take type, like k
    take listener
      like task
        take this, like svg-text-content-element
        take ev
          like index
            like svg-element-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    head k
      base key-list
        like svg-element-event-map

    take type, like k
    take listener
      like task
        take this, like svg-text-content-element
        take ev
          like index
            like svg-element-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void
