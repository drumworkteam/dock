
load @tunebond/moon
  take suit any
  take suit like

load @tunebond/bolt/code/javascript/native
  take face native-undefined

load @tunebond/bolt/code/javascript/dom
  take face transformer-flush-callback
  take face transformer-start-callback
  take face transformer-transform-callback

face transformer, name <Transformer>
  head i
    back any
  head o
    back any

  link flush, name <flush>
    like transformer-flush-callback
      like o
    flex true
  link readable-type, name <readableType>
    like native-undefined
    flex true
  link start, name <start>
    like transformer-start-callback
      like o
    flex true
  link transform, name <transform>
    like transformer-transform-callback
      like i
      like o
    flex true
  link writable-type, name <writableType>
    like native-undefined
    flex true
