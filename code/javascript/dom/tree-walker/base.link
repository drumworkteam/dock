
load @treesurf/moon
  take suit or

load @treesurf/nest/code/javascript/native
  take face native-null
  take face native-number

load @treesurf/nest/code/javascript/dom
  take face node
  take face node-filter

face tree-walker, name <TreeWalker>
  link current-node, name <currentNode>
    like node
    flex true
  link filter, name <filter>
    like or
      like node-filter
      like native-null
  link root, name <root>
    like node
  link what-to-show, name <whatToShow>
    like native-number

  task first-child, name <firstChild>
    free seed
      like or
        like node
        like native-null

  task last-child, name <lastChild>
    free seed
      like or
        like node
        like native-null

  task next-node, name <nextNode>
    free seed
      like or
        like node
        like native-null

  task next-sibling, name <nextSibling>
    free seed
      like or
        like node
        like native-null

  task parent-node, name <parentNode>
    free seed
      like or
        like node
        like native-null

  task previous-node, name <previousNode>
    free seed
      like or
        like node
        like native-null

  task previous-sibling, name <previousSibling>
    free seed
      like or
        like node
        like native-null
