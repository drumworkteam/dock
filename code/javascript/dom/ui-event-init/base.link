
load @tunebond/moon
  take suit or

load @tunebond/nest/code/javascript/native
  take face native-null
  take face native-number

load @tunebond/nest/code/javascript/dom
  take face event-init
  take face window

face ui-event-init, name <UIEventInit>
  base event-init

  link detail, name <detail>
    like native-number
    flex true
  link view, name <view>
    like or
      like window
      like native-null
    flex true
  link which, name <which>
    like native-number
    flex true
