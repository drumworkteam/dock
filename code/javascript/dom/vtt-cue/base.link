form vtt-cue, name <VTTCue>
  base text-track-cue

  take align, name <align>
    like align-setting
    lock free
  take line, name <line>
    like line-and-position-setting
    lock free
  take line-align, name <lineAlign>
    like line-align-setting
    lock free
  take position, name <position>
    like line-and-position-setting
    lock free
  take position-align
    name <positionAlign>
    like position-align-setting
    lock free
  take region, name <region>
    like sink-link-text
      like vtt-region
      like native-null
    lock free
  take size, name <size>
    like native-number
    lock free
  take snap-to-lines, name <snapToLines>
    like native-boolean
    lock free
  take text, name <text>
    like native-string
    lock free
  take vertical, name <vertical>
    like direction-setting
    lock free

  task get-cue-as-html
    name <getCueAsHTML>
    free seed, like document-fragment

  task add-event-listener
    name <addEventListener>
    head k, like like-link-text
      base link-read-link-text
        like text-track-cue-event-map
    take type, like k
    take listener
      like task
        take this, like vtt-cue
        take ev
          like form-link-link-text
            like text-track-cue-event-map
            like k
        free seed, like native-any
    take options
      like sink-link-text
        like native-boolean
        like add-event-listener-options
      void take
    free seed, like native-void

  task add-event-listener
    name <addEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like sink-link-text
        like native-boolean
        like add-event-listener-options
      void take
    free seed, like native-void

  task remove-event-listener
    name <removeEventListener>
    head k, like like-link-text
      base link-read-link-text
        like text-track-cue-event-map
    take type, like k
    take listener
      like task
        take this, like vtt-cue
        take ev
          like form-link-link-text
            like text-track-cue-event-map
            like k
        free seed, like native-any
    take options
      like sink-link-text
        like native-boolean
        like event-listener-options
      void take
    free seed, like native-void

  task remove-event-listener
    name <removeEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like sink-link-text
        like native-boolean
        like event-listener-options
      void take
    free seed, like native-void
