
load @tunebond/moon
  take suit any
  take suit index
  take suit key-list
  take suit like
  take suit or
  take suit task

load @tunebond/bolt/code/javascript/native
  take face native-boolean
  take face native-null
  take face native-number
  take face native-string
  take face native-void

load @tunebond/bolt/code/javascript/dom
  take face add-event-listener-options
  take face align-setting
  take face direction-setting
  take face document-fragment
  take face event-listener-options
  take face event-listener-or-event-listener-object
  take face line-align-setting
  take face line-and-position-setting
  take face position-align-setting
  take face text-track-cue
  take face text-track-cue-event-map
  take face vtt-region

face vtt-cue, name <VTTCue>
  base text-track-cue

  link align, name <align>
    like align-setting
    flex true
  link line, name <line>
    like line-and-position-setting
    flex true
  link line-align, name <lineAlign>
    like line-align-setting
    flex true
  link position, name <position>
    like line-and-position-setting
    flex true
  link position-align, name <positionAlign>
    like position-align-setting
    flex true
  link region, name <region>
    like or
      like vtt-region
      like native-null
    flex true
  link size, name <size>
    like native-number
    flex true
  link snap-to-lines, name <snapToLines>
    like native-boolean
    flex true
  link text, name <text>
    like native-string
    flex true
  link vertical, name <vertical>
    like direction-setting
    flex true

  task get-cue-as-html, name <getCueAsHTML>
    free seed, like document-fragment

  task add-event-listener, name <addEventListener>
    head k
      base key-list
        like text-track-cue-event-map

    take type, like k
    take listener
      like task
        take this, like vtt-cue
        take ev
          like index
            like text-track-cue-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task add-event-listener, name <addEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    head k
      base key-list
        like text-track-cue-event-map

    take type, like k
    take listener
      like task
        take this, like vtt-cue
        take ev
          like index
            like text-track-cue-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void

  task remove-event-listener, name <removeEventListener>
    take type, like native-string
    take listener, like event-listener-or-event-listener-object
    take options
      like or
        like native-boolean
        like event-listener-options
      void true
    free seed, like native-void
