
load @tunebond/bolt/code/javascript/dom
  take face close-event
  take face event
  take face message-event

face web-socket-event-map, name <WebSocketEventMap>
  link close, name <close>
    like close-event
    flex true
  link error, name <error>
    like event
    flex true
  link message, name <message>
    like message-event
    flex true
  link open, name <open>
    like event
    flex true
