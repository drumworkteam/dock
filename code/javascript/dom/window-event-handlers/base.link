  task add-event-listener, name <addEventListener>
    head k
      base key-list
        like window-event-handlers-event-map

    take type, like k
    take listener
      like task
        take this, like window-event-handlers
        take ev
          like index
            like window-event-handlers-event-map
            like k
        free seed, like any
    take options
      like or
        like native-boolean
        like add-event-listener-options
      void true
    free seed, like native-void

  task add-event-listener, name <addEventListener>
