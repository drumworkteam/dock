
load @tunebond/nest/code/javascript/dom
  take face device-motion-event
  take face device-orientation-event
  take face event
  take face gamepad-event
  take face global-event-handlers-event-map
  take face window-event-handlers-event-map

face window-event-map, name <WindowEventMap>
  base global-event-handlers-event-map
  base window-event-handlers-event-map

  link devicemotion, name <devicemotion>
    like device-motion-event
    flex true
  link deviceorientation, name <deviceorientation>
    like device-orientation-event
    flex true
  link gamepadconnected, name <gamepadconnected>
    like gamepad-event
    flex true
  link gamepaddisconnected, name <gamepaddisconnected>
    like gamepad-event
    flex true
  link orientationchange, name <orientationchange>
    like event
    flex true
