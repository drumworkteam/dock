
load @drumwork/land/code/javascript/base/native-null
  take form native-null

load @drumwork/land/code/javascript/base/native-string
  take form native-string

load @drumwork/seed/code/form/form
  take form form
    save form form-link-text

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form x-path-ns-resolver
  name <XPathNSResolver>
  like sink-link-text
    like task
      take prefix
        like sink-link-text
          like native-string
          like native-null
      free seed
        like sink-link-text
          like native-string
          like native-null
    like form-link-text
      task lookup-namespace-uri
        name <lookupNamespaceURI>
        take prefix
          like sink-link-text
            like native-string
            like native-null
        free seed
          like sink-link-text
            like native-string
            like native-null
