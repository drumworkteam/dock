
load @drumwork/land/code/javascript/base/form/native-any
  take form native-any

load @drumwork/land/code/javascript/base/form/native-number
  take form native-number

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/es5/base/form/array-like
  take form array-like

load @drumwork/land/code/javascript/es5/base/form/string-constructor
  take form string-constructor

load @drumwork/seed/code/form/form
  take form form
    save form form-link-text

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form string-constructor
  task from-code-point
    like task
      take code-points
        like list-link-text
          like native-number
        rest take
      free seed, like native-string

  task raw
    like task
      take template
        like form-link-text
          take raw
            like sink-link-text
              like list-link-text
                like native-string
              like array-like
                like native-string
            lock free
      take substitutions
        like list-link-text
          like native-any
        rest take
      free seed, like native-string
