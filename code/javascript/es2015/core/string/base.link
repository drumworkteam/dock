
load @tunebond/moon
  take suit any
  take suit form
  take suit or

load @tunebond/bolt/code/javascript/native
  take face native-boolean
  take face native-number
  take face native-string
  take face native-undefined

load @tunebond/bolt/code/javascript/es5/string
  take face string

load @tunebond/bolt/code/javascript
  take face array

load @tunebond/bolt/code/javascript/es5
  take face array-like

load @tunebond/bolt/code/javascript/es2015/core/string
  take face string

face string, name <String>
  task code-point-at, name <codePointAt>
    take pos, like native-number
    free seed
      like or
        like native-number
        like native-undefined

  task includes, name <includes>
    take search-string, like native-string
    take position, like native-number
      void true
    free seed, like native-boolean

  task ends-with, name <endsWith>
    take search-string, like native-string
    take end-position, like native-number
      void true
    free seed, like native-boolean

  task normalize, name <normalize>
    take form
      like or
        text <NFC>
        text <NFD>
        text <NFKC>
        text <NFKD>
    free seed, like native-string

  task normalize, name <normalize>
    take form, like native-string
      void true
    free seed, like native-string

  task repeat, name <repeat>
    take count, like native-number
    free seed, like native-string

  task starts-with, name <startsWith>
    take search-string, like native-string
    take position, like native-number
      void true
    free seed, like native-boolean

  task anchor, name <anchor>
    take name, like native-string
    free seed, like native-string

  task big, name <big>
    free seed, like native-string

  task blink, name <blink>
    free seed, like native-string

  task bold, name <bold>
    free seed, like native-string

  task fixed, name <fixed>
    free seed, like native-string

  task fontcolor, name <fontcolor>
    take color, like native-string
    free seed, like native-string

  task fontsize, name <fontsize>
    take size, like native-number
    free seed, like native-string

  task fontsize, name <fontsize>
    take size, like native-string
    free seed, like native-string

  task italics, name <italics>
    free seed, like native-string

  task link, name <link>
    take url, like native-string
    free seed, like native-string

  task small, name <small>
    free seed, like native-string

  task strike, name <strike>
    free seed, like native-string

  task sub, name <sub>
    free seed, like native-string

  task sup, name <sup>
    free seed, like native-string

face string
  hook self
    task from-code-point, name <fromCodePoint>
      take code-points
        like array
          like native-number
        rest true
      free seed, like native-string

    task raw, name <raw>
      take template
        like form
          link raw, name <raw>
            like or
              like array
                like native-string
              like array-like
                like native-string
            flex true
      take substitutions
        like array
          like any
        rest true
      free seed, like native-string
