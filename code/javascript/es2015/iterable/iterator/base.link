
load @tunebond/moon
  take suit any
  take suit like
  take suit or
  take suit tuple

load @tunebond/bolt/code/javascript/native
  take face native-undefined

load @tunebond/bolt/code/javascript/es2015/iterable
  take face iterator-result

face iterator, name <Iterator>
  head t
  head t-return
    back any
  head t-next
    back native-undefined

  task next, name <next>
    take args
      like or
        like tuple
        like tuple
          like t-next
      rest true
    free seed
      like iterator-result
        like t
        like t-return

  task return, name <return>
    take value, like t-return
      void true
    free seed
      like iterator-result
        like t
        like t-return

  task throw, name <throw>
    take e, like any
      void true
    free seed
      like iterator-result
        like t
        like t-return
