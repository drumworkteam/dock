
load @tunebond/bolt/code/javascript/native
  take face native-string

load @tunebond/bolt/code/javascript/es5/array-buffer
  take face array-buffer

load @tunebond/bolt/code/javascript/es2015/symbol/wellknown
  take face array-buffer-constructor

load @tunebond/bolt/code/javascript/es2015/symbol/wellknown/array-buffer
  take face array-buffer

face array-buffer, name <ArrayBuffer>
  link {symbol/to-string-tag}
    like native-string
    work true

face array-buffer
  hook self
    link {symbol/species}
      like array-buffer-constructor
      work true
