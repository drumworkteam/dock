
load @tunebond/moon
  take suit any
  take suit or
  take suit task

load @tunebond/nest/code/javascript
  take face array

load @tunebond/nest/code/javascript/native
  take face native-null
  take face native-number
  take face native-string

load @tunebond/nest/code/javascript/es5
  take face reg-exp-match-array

load @tunebond/nest/code/javascript/es2015/core/reg-exp
  take face reg-exp

load @tunebond/nest/code/javascript/es2015/symbol/wellknown
  take face reg-exp-constructor

load @tunebond/nest/code/javascript/es2015/symbol/wellknown/reg-exp
  take face reg-exp

face reg-exp, name <RegExp>
  task {symbol/match}
    take string, like native-string
    free seed
      like or
        like reg-exp-match-array
        like native-null

  task {symbol/replace}
    take string, like native-string
    take replace-value, like native-string
    free seed, like native-string

  task {symbol/replace}
    take string, like native-string
    take replacer
      like task
        take substring, like native-string
        take args
          like array
            like any
          rest true
        free seed, like native-string
    free seed, like native-string

  task {symbol/search}
    take string, like native-string
    free seed, like native-number

  task {symbol/split}
    take string, like native-string
    take limit, like native-number
      void true
    free seed
      like array
        like native-string

face reg-exp
  hook self
    link {symbol/species}
      like reg-exp-constructor
      work true
