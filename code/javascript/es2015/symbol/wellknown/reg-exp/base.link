
load @tunebond/bolt/code/javascript
  take form array

load @tunebond/moon
  take form any
  take form or
  take form task

load @tunebond/bolt/code/javascript/native
  take form native-null
  take form native-number
  take form native-string

load @tunebond/bolt/code/javascript/es5
  take form reg-exp-match-array

load @tunebond/bolt/code/javascript/es2015/core/reg-exp
  take form reg-exp

load @tunebond/bolt/code/javascript/es2015/symbol/wellknown
  take form reg-exp-constructor

load @tunebond/bolt/code/javascript/es2015/symbol/wellknown/reg-exp
  take form reg-exp

form reg-exp, name <RegExp>
  task {symbol/match}
    like or
      like reg-exp-match-array
      like native-null

    take string, like native-string

  task {symbol/replace}
    like native-string

    take string, like native-string
    take replace-value, like native-string

  task {symbol/replace}
    like native-string

    take string, like native-string
    take replacer
      like task
        like native-string

        take substring, like native-string
        take args
          like array
            like any
          rest true

  task {symbol/search}
    like native-number

    take string, like native-string

  task {symbol/split}
    like array
      like native-string

    take string, like native-string
    take limit, like native-number
      void true

form reg-exp
  hook self
    link {symbol/species}
      like reg-exp-constructor
      work true
