
load @tunebond/bolt/code/javascript/es2015/iterable
  take form iterator-result

load @tunebond/bolt/code/javascript/es2015/symbol/wellknown
  take form promise

load @tunebond/bolt/code/javascript/es5
  take form promise-like

load @tunebond/bolt/code/javascript/native
  take form async-iterator

form async-generator, name <AsyncGenerator>
  base async-iterator

  head t
    fall unknown
  head t-return
    fall any
  head t-next
    fall unknown

  task next, name <next>
    like promise
      like iterator-result
        like t
        like t-return

    take args
      like or
        like tuple
        like tuple
          like t-next
      rest true

  task return, name <return>
    like promise
      like iterator-result
        like t
        like t-return

    take value

  task throw, name <throw>
    like promise
      like iterator-result
        like t
        like t-return

    take e

  task {symbol/async-iterator}
    like async-generator
      like t
      like t-return
      like t-next
