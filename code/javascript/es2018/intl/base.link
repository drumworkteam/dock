
bear @tunebond/bolt/code/javascript/es2018/intl/ldml-plural-rule
bear @tunebond/bolt/code/javascript/es2018/intl/plural-rule-type
bear @tunebond/bolt/code/javascript/es2018/intl/plural-rules-options
bear @tunebond/bolt/code/javascript/es2018/intl/resolved-plural-rules-options
bear @tunebond/bolt/code/javascript/es2018/intl/plural-rules

load @tunebond/moon
  take suit form
  take suit or

load @tunebond/bolt/code/javascript
  take face array

load @tunebond/bolt/code/javascript/native
  take face native-string

load @tunebond/bolt/code/javascript/es2018/intl
  take face plural-rules
  take face plural-rules-options

host plural-rules, name <PluralRules>
  like form
    hook make
      take locales
        like or
          like native-string
          like array
            like native-string
        void true
      take options, like plural-rules-options
        void true
      free seed, like plural-rules

    task supported-locales-of, name <supportedLocalesOf>
      take locales
        like or
          like native-string
          like array
            like native-string
      take options, like plural-rules-options
        void true
      free seed
        like array
          like native-string
