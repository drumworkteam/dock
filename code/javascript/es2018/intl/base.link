
bear @treesurf/seed/code/javascript/es2018/intl/ldml-plural-rule
bear @treesurf/seed/code/javascript/es2018/intl/plural-rule-type
bear @treesurf/seed/code/javascript/es2018/intl/plural-rules-options
bear @treesurf/seed/code/javascript/es2018/intl/resolved-plural-rules-options
bear @treesurf/seed/code/javascript/es2018/intl/plural-rules

load @treesurf/seed/code/javascript/base/native-array
  take form native-array

load @treesurf/seed/code/javascript/base/native-string
  take form native-string

load @treesurf/seed/code/javascript/es2018/intl/plural-rules
  take form plural-rules

load @treesurf/seed/code/javascript/es2018/intl/plural-rules-options
  take form plural-rules-options

load @treesurf/seed/code/form/form
  take form form
    save form form-link-text

load @treesurf/seed/code/form/or
  take form or
    save form or-link-text

host plural-rules, name <PluralRules>
  like form-link-text
    hook make
      take locales
        like or-link-text
          like native-string
          like native-array
            like native-string
        void true
      take options, like plural-rules-options
        void true
      free seed, like plural-rules

    task supported-locales-of, name <supportedLocalesOf>
      take locales
        like or-link-text
          like native-string
          like native-array
            like native-string
      take options, like plural-rules-options
        void true
      free seed
        like native-array
          like native-string
