
load @drumwork/land/code/javascript/base/form/native-bigint
  take form native-bigint

load @drumwork/land/code/javascript/base/form/native-number
  take form native-number

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/es5/base/form/intl/number-format
  take form number-format

load @drumwork/land/code/javascript/es5/base/form/intl/resolved-number-format-options
  take form resolved-number-format-options

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form number-format
  task format
    like task
      take value
        like sink-link-text
          like native-number
          like native-bigint
      free seed, like native-string

  task resolved-options
    like task
      free seed, like resolved-number-format-options
