
load @drumwork/land/code/javascript/base/native-bigint
  take form native-bigint

load @drumwork/land/code/javascript/base/native-number
  take form native-number

load @drumwork/land/code/javascript/base/native-string
  take form native-string

load @drumwork/land/code/javascript/es5/number-format
  take form number-format

load @drumwork/land/code/javascript/es5/resolved-number-format-options
  take form resolved-number-format-options

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form number-format, name <NumberFormat>
  task format, name <format>
    take value
      like sink-link-text
        like native-number
        like native-bigint
    free seed, like native-string

  task resolved-options
    name <resolvedOptions>
    free seed, like resolved-number-format-options
