
load @treesurf/moon
  take form or

load @treesurf/nest/code/javascript/native
  take form native-bigint
  take form native-number
  take form native-string

load @treesurf/nest/code/javascript/es5
  take form resolved-number-format-options

load @treesurf/nest/code/javascript/es5/number-format
  take form number-format

form number-format, name <NumberFormat>
  task format, name <format>
    like native-string

    take value
      like or
        like native-number
        like native-bigint

  task resolved-options, name <resolvedOptions>
    like resolved-number-format-options
