
bear @tunebond/bolt/code/javascript/es2020/intl/unicode-bcp47-locale-identifier
bear @tunebond/bolt/code/javascript/es2020/intl/relative-time-format-unit
bear @tunebond/bolt/code/javascript/es2020/intl/relative-time-format-locale-matcher
bear @tunebond/bolt/code/javascript/es2020/intl/relative-time-format-numeric
bear @tunebond/bolt/code/javascript/es2020/intl/relative-time-format-style
bear @tunebond/bolt/code/javascript/es2020/intl/relative-time-format-options
bear @tunebond/bolt/code/javascript/es2020/intl/resolved-relative-time-format-options
bear @tunebond/bolt/code/javascript/es2020/intl/relative-time-format-part
bear @tunebond/bolt/code/javascript/es2020/intl/relative-time-format
bear @tunebond/bolt/code/javascript/es2020/intl/number-format-options
bear @tunebond/bolt/code/javascript/es2020/intl/resolved-number-format-options
bear @tunebond/bolt/code/javascript/es2020/intl/date-time-format-options

load @tunebond/moon
  take suit form
  take suit or

load @tunebond/bolt/code/javascript
  take face array

load @tunebond/bolt/code/javascript/es2020/intl
  take face relative-time-format
  take face relative-time-format-options
  take face unicode-bcp47-locale-identifier

host relative-time-format, name <RelativeTimeFormat>
  like form
    hook make
      take locales
        like or
          like unicode-bcp47-locale-identifier
          like array
            like unicode-bcp47-locale-identifier
        void true
      take options, like relative-time-format-options
        void true
      free seed, like relative-time-format

    task supported-locales-of, name <supportedLocalesOf>
      take locales
        like or
          like unicode-bcp47-locale-identifier
          like array
            like unicode-bcp47-locale-identifier
        void true
      take options, like relative-time-format-options
        void true
      free seed
        like array
          like unicode-bcp47-locale-identifier
