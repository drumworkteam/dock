
load @tunebond/moon
  take suit or

load @tunebond/bolt/code/javascript/native
  take face native-string

load @tunebond/bolt/code/javascript/es5/intl/date-time-format-options
  take face date-time-format-options

face date-time-format-options, name <DateTimeFormatOptions>
  link date-style, name <dateStyle>
    like or
      text <full>
      text <long>
      text <medium>
      text <short>
    flex true
  link time-style, name <timeStyle>
    like or
      text <full>
      text <long>
      text <medium>
      text <short>
    flex true
  link calendar, name <calendar>
    like native-string
    flex true
  link day-period, name <dayPeriod>
    like or
      text <narrow>
      text <short>
      text <long>
    flex true
  link numbering-system, name <numberingSystem>
    like native-string
    flex true
  link hour-cycle, name <hourCycle>
    like or
      text <h11>
      text <h12>
      text <h23>
      text <h24>
    flex true
  link fractional-second-digits, name <fractionalSecondDigits>
    like or
      text <0>
      text <1>
      text <2>
      text <3>
    flex true
