
load @drumwork/land/code/javascript/base/form/native-number
  take form native-number

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/es2020/intl/form/intl/relative-time-format-part
  take form relative-time-format-part

load @drumwork/land/code/javascript/es2020/intl/form/intl/relative-time-format-unit
  take form relative-time-format-unit

load @drumwork/land/code/javascript/es2020/intl/form/intl/resolved-relative-time-format-options
  take form resolved-relative-time-format-options

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

form relative-time-format
  task format
    like task
      take value, like native-number
      take unit, like relative-time-format-unit
      free seed, like native-string

  task format-to-parts
    like task
      take value, like native-number
      take unit, like relative-time-format-unit
      free seed
        like list-link-text
          like relative-time-format-part

  task resolved-options
    like task
      free seed, like resolved-relative-time-format-options
