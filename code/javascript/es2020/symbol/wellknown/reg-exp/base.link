
load @tunebond/bolt/code/javascript/native
  take face native-string

load @tunebond/bolt/code/javascript/es2015/iterable
  take face iterable-iterator

load @tunebond/bolt/code/javascript/es2018/regexp
  take face reg-exp-match-array

load @tunebond/bolt/code/javascript/es2018/regexp/reg-exp
  take face reg-exp

face reg-exp, name <RegExp>
  task {symbol/match-all}
    take str, like native-string
    free seed
      like iterable-iterator
        like reg-exp-match-array
