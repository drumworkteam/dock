
load @treesurf/moon
  take form any
  take form like

load @treesurf/nest/code/javascript
  take form array

load @treesurf/nest/code/javascript/native
  take form boolean
    save form native-boolean
  take form number
    save form native-number

form array-constructor, name <ArrayConstructor>
  link prototype, name <prototype>
    like array
      like any

  task is-array, name <isArray>
    take arg, like any
    free seed
      like native-boolean
      rank arg
        like array
          like any

  hook make
    take array-length, like native-number
      void true
    free seed
      like array
        like any

  hook make
    head t

    take array-length, like native-number
    free seed
      like array
        like t

  hook make
    head t

    take items
      like array
        like t
      rest true
    free seed
      like array
        like t
