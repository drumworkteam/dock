  task atan, text <atan>
  task atan2, text <atan2>
  task ceil, text <ceil>
  task cos, text <cos>
  task exp, text <exp>
  task floor, text <floor>
  task log, text <log>
  task max, text <max>
  task min, text <min>
  task pow, text <pow>
  task random, text <random>
  task round, text <round>
