
load @treesurf/moon
  take form any
  take form like
  take form task

load @treesurf/nest/code/javascript
  take form array

load @treesurf/nest/code/javascript/es5
  take form function
  take form omit-this-parameter
  take form this-parameter-type

form callable-function, name <CallableFunction>
  base function

  task apply, name <apply>
    like r

    head t
    head r

    take this
      like task
        like r

        take this, like t
    take this-arg, like t

  task apply, name <apply>
    like r

    head t
    head a
      base array
        like any
    head r

    take this
      like task
        like r

        take this, like t
        take args, like a
          rest true
    take this-arg, like t
    take args, like a

  task call, name <call>
    like r

    head t
    head a
      base array
        like any
    head r

    take this
      like task
        like r

        take this, like t
        take args, like a
          rest true
    take this-arg, like t
    take args, like a
      rest true

  task bind, name <bind>
    like omit-this-parameter
      like t

    head t

    take this, like t
    take this-arg
      like this-parameter-type
        like t

  task bind, name <bind>
    like task
      like r

      take args, like a
        rest true

    head t
    head a0
    head a
      base array
        like any
    head r

    take this
      like task
        like r

        take this, like t
        take arg0, like a0
        take args, like a
          rest true
    take this-arg, like t
    take arg0, like a0

  task bind, name <bind>
    like task
      like r

      take args, like a
        rest true

    head t
    head a0
    head a1
    head a
      base array
        like any
    head r

    take this
      like task
        like r

        take this, like t
        take arg0, like a0
        take arg1, like a1
        take args, like a
          rest true
    take this-arg, like t
    take arg0, like a0
    take arg1, like a1

  task bind, name <bind>
    like task
      like r

      take args, like a
        rest true

    head t
    head a0
    head a1
    head a2
    head a
      base array
        like any
    head r

    take this
      like task
        like r

        take this, like t
        take arg0, like a0
        take arg1, like a1
        take arg2, like a2
        take args, like a
          rest true
    take this-arg, like t
    take arg0, like a0
    take arg1, like a1
    take arg2, like a2

  task bind, name <bind>
    like task
      like r

      take args, like a
        rest true

    head t
    head a0
    head a1
    head a2
    head a3
    head a
      base array
        like any
    head r

    take this
      like task
        like r

        take this, like t
        take arg0, like a0
        take arg1, like a1
        take arg2, like a2
        take arg3, like a3
        take args, like a
          rest true
    take this-arg, like t
    take arg0, like a0
    take arg1, like a1
    take arg2, like a2
    take arg3, like a3

  task bind, name <bind>
    like task
      like r

      take args
        like array
          like ax
        rest true

    head t
    head ax
    head r

    take this
      like task
        like r

        take this, like t
        take args
          like array
            like ax
          rest true
    take this-arg, like t
    take args
      like array
        like ax
      rest true
