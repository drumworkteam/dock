
load @tunebond/bolt/code/javascript
  take form array

load @tunebond/bolt/code/javascript/native
  take form native-number
  take form native-string

load @tunebond/bolt/code/javascript/es5/function
  take form function

form function, name <Function>
  link prototype, name <prototype>
    like any
    flex true
  link length, name <length>
    like native-number
  link arguments, name <arguments>
    like any
    flex true
  link caller, name <caller>
    like function
    flex true

  task apply, name <apply>
    like any

    take this
    take this-arg
    take arg-array, like maybe, like any

  task call, name <call>
    like any

    take this
    take this-arg
    take arg-array
      like array
        like any
      rest true

  task bind, name <bind>
    like any

    take this
    take this-arg
    take arg-array
      like array
        like any
      rest true

  task to-string, name <toString>
    like native-string

form function
  hook self
    link prototype, name <prototype>
      like function

  hook make
    like function

    take args
      like array
        like native-string
      rest true
