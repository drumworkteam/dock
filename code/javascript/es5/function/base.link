
load @tunebond/moon
  take suit any

load @tunebond/bolt/code/javascript
  take face array

load @tunebond/bolt/code/javascript/native
  take face native-number
  take face native-string

load @tunebond/bolt/code/javascript/es5/function
  take face function

face function, name <Function>
  link prototype, name <prototype>
    like any
    flex true
  link length, name <length>
    like native-number
  link arguments, name <arguments>
    like any
    flex true
  link caller, name <caller>
    like function
    flex true

  task apply, name <apply>
    take this, like function
    take this-arg, like any
    take arg-array, like any
      void true
    free seed, like any

  task call, name <call>
    take this, like function
    take this-arg, like any
    take arg-array
      like array
        like any
      rest true
    free seed, like any

  task bind, name <bind>
    take this, like function
    take this-arg, like any
    take arg-array
      like array
        like any
      rest true
    free seed, like any

  task to-string, name <toString>
    free seed, like native-string

face function
  hook self
    link prototype, name <prototype>
      like function

  hook make
    take args
      like array
        like native-string
      rest true
    free seed, like function
