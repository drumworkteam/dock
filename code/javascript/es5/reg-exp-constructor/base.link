
load @treesurf/nest/code/javascript/base/native-string
  take form native-string

load @treesurf/nest/code/javascript/es5/reg-exp
  take form reg-exp

load @treesurf/nest/code/form/or
  take form or
    save form or-link-text

form reg-exp-constructor, name <RegExpConstructor>
  take prototype, name <prototype>
    like reg-exp
  take 1, name <$1>
    like native-string
    lock false
  take 2, name <$2>
    like native-string
    lock false
  take 3, name <$3>
    like native-string
    lock false
  take 4, name <$4>
    like native-string
    lock false
  take 5, name <$5>
    like native-string
    lock false
  take 6, name <$6>
    like native-string
    lock false
  take 7, name <$7>
    like native-string
    lock false
  take 8, name <$8>
    like native-string
    lock false
  take 9, name <$9>
    like native-string
    lock false
  take last-match, name <lastMatch>
    like native-string
    lock false

  hook make
    take pattern
      like or-link-text
        like reg-exp
        like native-string
    free seed, like reg-exp

  hook make
    take pattern, like native-string
    take flags, like native-string
      void true
    free seed, like reg-exp
