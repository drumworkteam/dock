
load @tunebond/moon
  take suit like
  take suit task

load @tunebond/nest/code/javascript/native
  take face native-boolean
  take face native-void

face typed-property-descriptor, name <TypedPropertyDescriptor>
  head t

  link enumerable, name <enumerable>
    like native-boolean
    flex true
  link configurable, name <configurable>
    like native-boolean
    flex true
  link writable, name <writable>
    like native-boolean
    flex true
  link value, name <value>
    like t
    flex true
  link get, name <get>
    like task
      free seed, like t
    flex true
  link set, name <set>
    like task
      take value, like t
      free seed, like native-void
    flex true
