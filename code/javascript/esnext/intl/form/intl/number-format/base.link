
load @drumwork/land/code/javascript/base/form/native-bigint
  take form native-bigint

load @drumwork/land/code/javascript/base/form/native-number
  take form native-number

load @drumwork/land/code/javascript/es2020/bigint/form/intl/number-format
  take form number-format

load @drumwork/land/code/javascript/esnext/intl/form/intl/number-format-part
  take form number-format-part

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form number-format
  task format-to-parts
    like task
      take number
        like sink-link-text
          like native-number
          like native-bigint
        void take
      free seed
        like list-link-text
          like number-format-part
