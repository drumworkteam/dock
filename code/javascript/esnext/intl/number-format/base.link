
load @treesurf/moon
  take suit or

load @treesurf/nest/code/javascript
  take face array

load @treesurf/nest/code/javascript/native
  take face native-bigint
  take face native-number

load @treesurf/nest/code/javascript/esnext/intl
  take face number-format-part

load @treesurf/nest/code/javascript/es2020/bigint/number-format
  take face number-format

face number-format, name <NumberFormat>
  task format-to-parts, name <formatToParts>
    take number
      like or
        like native-number
        like native-bigint
      void true
    free seed
      like array
        like number-format-part
