
load @drumwork/moon/code/javascript/base/native-array
  take form native-array

load @drumwork/moon/code/javascript/base/native-bigint
  take form native-bigint

load @drumwork/moon/code/javascript/base/native-number
  take form native-number

load @drumwork/moon/code/javascript/es2020/bigint/number-format
  take form number-format

load @drumwork/moon/code/javascript/esnext/intl/number-format-part
  take form number-format-part

load @drumwork/seed/code/form/or
  take form or
    save form or-link-text

form number-format, name <NumberFormat>
  task format-to-parts, name <formatToParts>
    take number
      like or-link-text
        like native-number
        like native-bigint
      void true
    free seed
      like native-array
        like number-format-part
