
load @treesurf/moon
  take form any
  take form task

load @treesurf/nest/code/javascript/native
  take form void
    save form native-void

load @treesurf/nest/code/javascript/esnext/weakref
  take form finalization-registry

load @treesurf/nest/code/javascript/es2021/weakref/finalization-registry-constructor
  take form finalization-registry-constructor

form finalization-registry-constructor, name <FinalizationRegistryConstructor>
