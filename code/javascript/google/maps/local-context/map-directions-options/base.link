
load @treesurf/nest/code/javascript/base/function
  take form function

load @treesurf/nest/code/javascript/base/native-string
  take form native-string

load @treesurf/nest/code/javascript/google/maps/lat-lng
  take form lat-lng

load @treesurf/nest/code/javascript/google/maps/lat-lng-literal
  take form lat-lng-literal

load @treesurf/nest/code/javascript/google/maps/maps-event-listener
  take form maps-event-listener

load @treesurf/nest/code/form/or
  take form or
    save form or-link-text

form map-directions-options, name <google.maps.localContext.MapDirectionsOptions>
  take origin
    like or-link-text
      like lat-lng
      like lat-lng-literal

  task add-listener, name <addListener>
    take event-name, like native-string
    take handler, like function
    free seed, like maps-event-listener
