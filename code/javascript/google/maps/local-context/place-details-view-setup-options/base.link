
load @drumwork/moon/code/javascript/base/native-boolean
  take form native-boolean

load @drumwork/moon/code/javascript/base/native-null
  take form native-null

load @drumwork/moon/code/javascript/google/maps/local-context/place-details-layout-mode
  take form place-details-layout-mode

load @drumwork/moon/code/javascript/google/maps/local-context/place-details-position
  take form place-details-position

load @drumwork/seed/code/form/or
  take form or
    save form or-link-text

form place-details-view-setup-options, name <google.maps.localContext.PlaceDetailsViewSetupOptions>
  take hides-on-map-click, name <hidesOnMapClick>
    like native-boolean
    lock false
  take layout-mode, name <layoutMode>
    like or-link-text
      like local-context-place-details-layout-mode
      like native-null
    lock false
  take position, name <position>
    like or-link-text
      like local-context-place-details-position
      like native-null
    lock false
