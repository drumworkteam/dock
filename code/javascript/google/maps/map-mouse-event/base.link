
load @drumwork/moon/code/javascript/base/event
  take form event

load @drumwork/moon/code/javascript/base/keyboard-event
  take form keyboard-event

load @drumwork/moon/code/javascript/base/native-null
  take form native-null

load @drumwork/moon/code/javascript/base/native-void
  take form native-void

load @drumwork/moon/code/javascript/base/pointer-event
  take form pointer-event

load @drumwork/moon/code/javascript/base/touch-event
  take form touch-event

load @drumwork/moon/code/javascript/google/maps/data/mouse-event
  take form mouse-event

load @drumwork/moon/code/javascript/google/maps/lat-lng
  take form lat-lng

load @drumwork/seed/code/form/or
  take form or
    save form or-link-text

form map-mouse-event, name <google.maps.MapMouseEvent>
  take dom-event, name <domEvent>
    like or-link-text
      like mouse-event
      like touch-event
      like pointer-event
      like keyboard-event
      like event
    lock false
  take lat-lng, name <latLng>
    like or-link-text
      like lat-lng
      like native-null
    lock false

  task stop, name <stop>
    free seed, like native-void
