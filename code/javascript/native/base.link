
form native-boolean

form native-string

form native-number

form native-null

form native-undefined

form native-this

form native-symbol

form native-object

form native-bigint

task test
  take test, like force
  take block, like force

  call native-test
    bind test, link test
    bind block, link block

task test-else
  take test, like force
  take block, like force
  take other, like force

  call native-test-else
    bind test, link test
    bind block, link block
    bind else, link other

task debug-break
  call debug
    bind keyword, text <debugger>

task compute-bitwise-or
  take left
  take right

  call compute-binary-operation
    bind left, link left
    bind operation, text <|>
    bind right, link right
    turn seed

task test-if-equal
  take left
  take right

  call compute-binary-operation
    bind left, link left
    bind operation, text <==>
    bind right, link right
    turn seed

task test-if-null
  take value

  call compute-binary-operation
    bind left, link value
    bind operation, text <==>
    bind right, text <null>
    turn seed

task test-if-strictly-equal
  take left
  take right

  call compute-binary-operation
    bind left, link left
    bind operation, text <===>
    bind right, link right
    turn seed

task get-typeof
  take value

  call call-keyword
    bind keyword, text <typeof>
    bind value, link value
    turn seed

task get-instanceof
  take left
  take right

  call compute-binary-operation
