
load @tunebond/moon
  take form any
  take form like
  take form or
  take form task

load @tunebond/bolt/code/javascript
  take form array
  take form native-undefined
  take form native-void
  take form t

form async-local-storage, name <AsyncLocalStorage>
  task disable, name <disable>
    free seed, like native-void

  task get-store, name <getStore>
    free seed
      like or
        like t
        like native-undefined

  task run, name <run>
    head r
    head t-args
      base array
        like any

    take store, like t
    take callback
      like task
        take args, like t-args
          rest true
        free result, like r
    take args, like t-args
      rest true
    free seed, like r

  task exit, name <exit>
    head r
    head t-args
      base array
        like any

    take callback
      like task
        take args, like t-args
          rest true
        free result, like r
    take args, like t-args
      rest true
    free seed, like r

  task enter-with, name <enterWith>
    take store, like t
    free seed, like native-void
