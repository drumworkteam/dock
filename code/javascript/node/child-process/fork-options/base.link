
load @treesurf/nest/code/javascript/base/abortable
  take form abortable

load @treesurf/nest/code/javascript/base/native-array
  take form native-array

load @treesurf/nest/code/javascript/base/native-boolean
  take form native-boolean

load @treesurf/nest/code/javascript/base/native-string
  take form native-string

load @treesurf/nest/code/javascript/base/native-undefined
  take form native-undefined

load @treesurf/nest/code/javascript/node/child-process/messaging-options
  take form messaging-options

load @treesurf/nest/code/javascript/node/child-process/process-env-options
  take form process-env-options

load @treesurf/nest/code/javascript/node/child-process/stdio-options
  take form stdio-options

load @treesurf/nest/code/form/or
  take form or
    save form or-link-text

form fork-options, name <ForkOptions>
  base process-env-options
  base messaging-options
  base abortable

  take exec-path, name <execPath>
    like or-link-text
      like native-string
      like native-undefined
    lock false
  take exec-argv, name <execArgv>
    like or-link-text
      like native-array
        like native-string
      like native-undefined
    lock false
  take silent, name <silent>
    like or-link-text
      like native-boolean
      like native-undefined
    lock false
  take stdio, name <stdio>
    like or-link-text
      like stdio-options
      like native-undefined
    lock false
  take detached, name <detached>
    like or-link-text
      like native-boolean
      like native-undefined
    lock false
  take windows-verbatim-arguments, name <windowsVerbatimArguments>
    like or-link-text
      like native-boolean
      like native-undefined
    lock false
