
load @treesurf/moon
  take form or

load @treesurf/nest/code/javascript
  take form native-number
  take form native-string

form address, name <Address>
  link address, name <address>
    like native-string
    flex true
  link port, name <port>
    like native-number
    flex true
  link address-type, name <addressType>
    like or
      like native-number
      text <udp4>
      text <udp6>
    flex true
