
load @treesurf/seed/code/javascript/base/native-boolean
  take form native-boolean

load @treesurf/seed/code/javascript/base/native-undefined
  take form native-undefined

load @treesurf/seed/code/javascript/node/crypto/large-number-like
  take form large-number-like

load @treesurf/seed/code/form/or
  take form or
    save form or-link-text

form generate-prime-options, name <GeneratePrimeOptions>
  take add, name <add>
    like or-link-text
      like large-number-like
      like native-undefined
    lock false
  take rem, name <rem>
    like or-link-text
      like large-number-like
      like native-undefined
    lock false
  take safe, name <safe>
    like or-link-text
      like native-boolean
      like native-undefined
    lock false
  take bigint, name <bigint>
    like or-link-text
      like native-boolean
      like native-undefined
    lock false
