
load @treesurf/seed/code/javascript/base/native-string
  take form native-string

load @treesurf/seed/code/javascript/base/native-undefined
  take form native-undefined

load @treesurf/seed/code/javascript/node/buffer/buffer
  take form buffer

load @treesurf/seed/code/javascript/node/crypto/key-format
  take form key-format

load @treesurf/seed/code/form/or
  take form or
    save form or-link-text

form private-key-input, name <PrivateKeyInput>
  take key, name <key>
    like or-link-text
      like native-string
      like buffer
    lock false
  take format, name <format>
    like or-link-text
      like key-format
      like native-undefined
    lock false
  take type, name <type>
    like or-link-text
      text <pkcs1>
      text <pkcs8>
      text <sec1>
      like native-undefined
    lock false
  take passphrase, name <passphrase>
    like or-link-text
      like native-string
      like buffer
      like native-undefined
    lock false
