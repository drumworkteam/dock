
load @treesurf/seed/code/javascript/base/native-string
  take form native-string

load @treesurf/seed/code/javascript/base/native-this
  take form native-this

load @treesurf/seed/code/javascript/node/buffer/buffer
  take form buffer

load @treesurf/seed/code/javascript/node/crypto/binary-like
  take form binary-like

load @treesurf/seed/code/javascript/node/crypto/binary-to-text-encoding
  take form binary-to-text-encoding

load @treesurf/seed/code/javascript/node/crypto/encoding
  take form encoding

load @treesurf/seed/code/javascript/node/crypto/key-like
  take form key-like

load @treesurf/seed/code/javascript/node/crypto/sign-key-object-input
  take form sign-key-object-input

load @treesurf/seed/code/javascript/node/crypto/sign-private-key-input
  take form sign-private-key-input

load @treesurf/seed/code/form/or
  take form or
    save form or-link-text

form sign, name <Sign>
  base stream-writable
  task update, name <update>
    take data, like binary-like
    free seed, like native-this

  task update, name <update>
    take data, like native-string
    take input-encoding, like encoding
    free seed, like native-this

  task sign, name <sign>
    take private-key
      like or-link-text
        like key-like
        like sign-key-object-input
        like sign-private-key-input
    free seed, like buffer

  task sign, name <sign>
    take private-key
      like or-link-text
        like key-like
        like sign-key-object-input
        like sign-private-key-input
    take output-format, like binary-to-text-encoding
    free seed, like native-string

form sign-constructor
  hook make
