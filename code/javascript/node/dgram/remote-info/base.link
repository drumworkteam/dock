
load @treesurf/moon
  take form or

load @drumwork/nest/code/javascript
  take form number
    save form native-number
  take form string
    save form native-string

form remote-info, name <RemoteInfo>
  link address, name <address>
    like native-string
    flex true
  link family, name <family>
    like or
      text <IPv4>
      text <IPv6>
    flex true
  link port, name <port>
    like native-number
    flex true
  link size, name <size>
    like native-number
    flex true
