
load @tunebond/moon
  take form or

load @tunebond/nest/code/javascript
  take form native-string
  take form native-void
  take form promise
  take form uint8-array

load @tunebond/nest/code/javascript/node/fs
  take form path-or-file-descriptor
  take form write-file-options

task promisify, name <__promisify__>
  take file, like path-or-file-descriptor
  take data
    like or
      like native-string
      like uint8-array
  take options, like write-file-options
    void true
  free seed
    like promise
      like native-void
