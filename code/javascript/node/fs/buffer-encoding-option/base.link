
load @treesurf/nest/code/form/form
  take form form
    save form form-link-text

load @treesurf/nest/code/form/or
  take form or
    save form or-link-text

form buffer-encoding-option, name <BufferEncodingOption>
  like or-link-text
    text <buffer>
    like form-link-text
      take encoding, name <encoding>
        text <buffer>
        lock false
