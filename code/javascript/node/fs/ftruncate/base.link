
load @tunebond/moon
  take form or

load @tunebond/nest/code/javascript
  take form native-null
  take form native-number
  take form native-void
  take form promise

task promisify, name <__promisify__>
  take fd, like native-number
  take len
    like or
      like native-number
      like native-null
    void true
  free seed
    like promise
      like native-void
