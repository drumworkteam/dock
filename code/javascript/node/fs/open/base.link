
load @tunebond/moon
  take form or

load @tunebond/nest/code/javascript
  take form native-null
  take form native-number
  take form promise

load @tunebond/nest/code/javascript/node/fs
  take form mode
  take form open-mode
  take form path-like

task promisify, name <__promisify__>
  take path, like path-like
  take flags, like open-mode
  take mode
    like or
      like mode
      like native-null
    void true
  free seed
    like promise
      like native-number
