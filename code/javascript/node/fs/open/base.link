
load @drumwork/moon/code/javascript/base/native-null
  take form native-null

load @drumwork/moon/code/javascript/base/native-number
  take form native-number

load @drumwork/moon/code/javascript/base/promise
  take form promise

load @drumwork/moon/code/javascript/node/fs/mode
  take form mode

load @drumwork/moon/code/javascript/node/fs/open-mode
  take form open-mode

load @drumwork/moon/code/javascript/node/fs/path-like
  take form path-like

load @drumwork/seed/code/form/or
  take form or
