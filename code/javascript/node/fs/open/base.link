
load @treesurf/moon
  take form or

load @drumwork/nest/code/javascript
  take form null
    save form native-null
  take form number
    save form native-number
  take form promise

load @drumwork/nest/code/javascript/node/fs
  take form mode
  take form open-mode
  take form path-like

task promisify, name <__promisify__>
  take path, like path-like
  take flags, like open-mode
  take mode
    like or
      like mode
      like native-null
    void true
  free seed
    like promise
      like native-number
