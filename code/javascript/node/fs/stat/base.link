
load @treesurf/moon
  take form and
  take form form
  take form or

load @drumwork/nest/code/javascript
  take form promise
  take form undefined
    save form native-undefined

load @drumwork/nest/code/javascript/node/fs
  take form big-int-stats
  take form path-like
  take form stat-options
  take form stats

task promisify, name <__promisify__>
  take path, like path-like
  take options
    like and
      like stat-options
      like form
        link bigint, name <bigint>
          like or
            text <false>
            like native-undefined
          flex true
    void true
  free seed
    like promise
      like stats

task promisify, name <__promisify__>
  take path, like path-like
  take options
    like and
      like stat-options
      like form
        link bigint, name <bigint>
          text <true>
          flex true
  free seed
    like promise
      like big-int-stats

task promisify, name <__promisify__>
  take path, like path-like
  take options, like stat-options
    void true
  free seed
    like promise
      like or
        like stats
        like big-int-stats
