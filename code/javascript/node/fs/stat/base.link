
load @treesurf/seed/code/javascript/base/native-undefined
  take form native-undefined

load @treesurf/seed/code/javascript/base/promise
  take form promise

load @treesurf/seed/code/javascript/node/fs/big-int-stats
  take form big-int-stats

load @treesurf/seed/code/javascript/node/fs/path-like
  take form path-like

load @treesurf/seed/code/javascript/node/fs/stat-options
  take form stat-options

load @treesurf/seed/code/javascript/node/fs/stats
  take form stats

load @treesurf/seed/code/form/and
  take form and
    save form and-link-text

load @treesurf/seed/code/form/form
  take form form
    save form form-link-text

load @treesurf/seed/code/form/or
  take form or
    save form or-link-text

task promisify, name <__promisify__>
  take path, like path-like
  take options
    like and-link-text
      like stat-options
      like form-link-text
        take bigint, name <bigint>
          like or-link-text
            text <false>
            like native-undefined
          lock false
    void true
  free seed
    like promise
      like stats

task promisify, name <__promisify__>
  take path, like path-like
  take options
    like and-link-text
      like stat-options
      like form-link-text
        take bigint, name <bigint>
          text <true>
          lock false
  free seed
    like promise
      like big-int-stats

task promisify, name <__promisify__>
  take path, like path-like
  take options, like stat-options
    void true
  free seed
    like promise
      like or-link-text
        like stats
        like big-int-stats
