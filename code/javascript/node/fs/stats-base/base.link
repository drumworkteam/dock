
load @drumwork/moon/code/javascript/base/date
  take form date

load @drumwork/moon/code/javascript/base/native-boolean
  take form native-boolean

load @drumwork/seed/code/form/like
  take form like
    save form like-link-text

form stats-base, name <StatsBase>
  head t, like like-link-text

  take dev, name <dev>
    like t
    lock false
  take ino, name <ino>
    like t
    lock false
  take mode, name <mode>
    like t
    lock false
  take nlink, name <nlink>
    like t
    lock false
  take uid, name <uid>
    like t
    lock false
  take gid, name <gid>
    like t
    lock false
  take rdev, name <rdev>
    like t
    lock false
  take size, name <size>
    like t
    lock false
  take blksize, name <blksize>
    like t
    lock false
  take blocks, name <blocks>
    like t
    lock false
  take atime-ms, name <atimeMs>
    like t
    lock false
  take mtime-ms, name <mtimeMs>
    like t
    lock false
  take ctime-ms, name <ctimeMs>
    like t
    lock false
  take birthtime-ms, name <birthtimeMs>
    like t
    lock false
  take atime, name <atime>
    like date
    lock false
  take mtime, name <mtime>
    like date
    lock false
  take ctime, name <ctime>
    like date
    lock false
  take birthtime, name <birthtime>
    like date
    lock false

  task is-file, name <isFile>
    free seed, like native-boolean

  task is-directory, name <isDirectory>
    free seed, like native-boolean

  task is-block-device, name <isBlockDevice>
    free seed, like native-boolean

  task is-character-device, name <isCharacterDevice>
    free seed, like native-boolean

  task is-symbolic-link, name <isSymbolicLink>
    free seed, like native-boolean

  task is-fifo, name <isFIFO>
    free seed, like native-boolean

  task is-socket, name <isSocket>
    free seed, like native-boolean
