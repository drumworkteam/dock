
load @treesurf/moon
  take form or

load @drumwork/nest/code/javascript
  take form null
    save form native-null
  take form number
    save form native-number
  take form promise
  take form void
    save form native-void

load @drumwork/nest/code/javascript/node/fs
  take form path-like

task promisify, name <__promisify__>
  take path, like path-like
  take len
    like or
      like native-number
      like native-null
    void true
  free seed
    like promise
      like native-void
