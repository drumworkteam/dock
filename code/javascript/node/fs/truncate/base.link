
load @tunebond/moon
  take form or

load @tunebond/bolt/code/javascript
  take form native-null
  take form native-number
  take form native-void
  take form promise

load @tunebond/bolt/code/javascript/node/fs
  take form path-like

task promisify, name <__promisify__>
  take path, like path-like
  take len
    like or
      like native-number
      like native-null
    void true
  free seed
    like promise
      like native-void
