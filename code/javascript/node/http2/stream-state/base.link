
load @treesurf/nest/code/javascript/base/native-number
  take form native-number

load @treesurf/nest/code/javascript/base/native-undefined
  take form native-undefined

load @treesurf/nest/code/form/or
  take form or
    save form or-link-text

form stream-state, name <StreamState>
  take local-window-size, name <localWindowSize>
    like or-link-text
      like native-number
      like native-undefined
    lock false
  take state, name <state>
    like or-link-text
      like native-number
      like native-undefined
    lock false
  take local-close, name <localClose>
    like or-link-text
      like native-number
      like native-undefined
    lock false
  take remote-close, name <remoteClose>
    like or-link-text
      like native-number
      like native-undefined
    lock false
  take sum-dependency-weight, name <sumDependencyWeight>
    like or-link-text
      like native-number
      like native-undefined
    lock false
  take weight, name <weight>
    like or-link-text
      like native-number
      like native-undefined
    lock false
