
load @treesurf/moon
  take form and
  take form form
  take form or

load @drumwork/nest/code/javascript
  take form boolean
    save form native-boolean
  take form http-request-options
  take form string
    save form native-string
  take form tls-secure-context-options
  take form undefined
    save form native-undefined

form request-options, name <RequestOptions>
  like and
    like http-request-options
    like tls-secure-context-options
    like form
      link reject-unauthorized, name <rejectUnauthorized>
        like or
          like native-boolean
          like native-undefined
        flex true
      link servername, name <servername>
        like or
          like native-string
          like native-undefined
        flex true