
load @treesurf/moon
  take form or

load @drumwork/nest/code/javascript
  take form boolean
    save form native-boolean
  take form runtime-script-id
  take form string
    save form native-string
  take form undefined
    save form native-undefined

form search-in-content-parameter-type, name <SearchInContentParameterType>
  link script-id, name <scriptId>
    like runtime-script-id
    flex true
  link query, name <query>
    like native-string
    flex true
  link case-sensitive, name <caseSensitive>
    like or
      like native-boolean
      like native-undefined
    flex true
  link is-regex, name <isRegex>
    like or
      like native-boolean
      like native-undefined
    flex true
