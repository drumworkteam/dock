
load @tunebond/moon
  take form or

load @tunebond/bolt/code/javascript
  take form native-string
  take form promise
  take form url

form import-meta, name <ImportMeta>
  home true
  link url, name <url>
    like native-string
    flex true

  task resolve, name <resolve>
    take specified, like native-string
    take parent
      like or
        like native-string
        like url
      void true
    free seed
      like promise
        like native-string
