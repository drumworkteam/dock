
load @drumwork/land/code/javascript/base/native-boolean
  take form native-boolean

load @drumwork/land/code/javascript/base/native-number
  take form native-number

load @drumwork/land/code/javascript/base/native-string
  take form native-string

load @drumwork/land/code/javascript/base/native-undefined
  take form native-undefined

load @drumwork/land/code/javascript/node/events/event-emitter/abortable
  take form abortable

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form listen-options
  name <ListenOptions>
  base abortable

  take port, name <port>
    like sink-link-text
      like native-number
      like native-undefined
    lock free
  take host, name <host>
    like sink-link-text
      like native-string
      like native-undefined
    lock free
  take backlog, name <backlog>
    like sink-link-text
      like native-number
      like native-undefined
    lock free
  take path, name <path>
    like sink-link-text
      like native-string
      like native-undefined
    lock free
  take exclusive, name <exclusive>
    like sink-link-text
      like native-boolean
      like native-undefined
    lock free
  take readable-all, name <readableAll>
    like sink-link-text
      like native-boolean
      like native-undefined
    lock free
  take writable-all, name <writableAll>
    like sink-link-text
      like native-boolean
      like native-undefined
    lock free
  take ipv6-only, name <ipv6Only>
    like sink-link-text
      like native-boolean
      like native-undefined
    lock free
