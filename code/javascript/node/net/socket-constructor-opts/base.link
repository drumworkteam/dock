
load @treesurf/moon
  take form or

load @drumwork/nest/code/javascript
  take form boolean
    save form native-boolean
  take form number
    save form native-number
  take form undefined
    save form native-undefined

load @drumwork/nest/code/javascript/node/globals
  take form abort-signal

form socket-constructor-opts, name <SocketConstructorOpts>
  link fd, name <fd>
    like or
      like native-number
      like native-undefined
    flex true
  link allow-half-open, name <allowHalfOpen>
    like or
      like native-boolean
      like native-undefined
    flex true
  link readable, name <readable>
    like or
      like native-boolean
      like native-undefined
    flex true
  link writable, name <writable>
    like or
      like native-boolean
      like native-undefined
    flex true
  link signal, name <signal>
    like abort-signal
    flex true
