
load @treesurf/moon
  take form or

load @drumwork/nest/code/javascript
  take form boolean
    save form native-boolean
  take form null
    save form native-null
  take form string
    save form native-string

form network-interface-base, name <NetworkInterfaceBase>
  link address, name <address>
    like native-string
    flex true
  link netmask, name <netmask>
    like native-string
    flex true
  link mac, name <mac>
    like native-string
    flex true
  link internal, name <internal>
    like native-boolean
    flex true
  link cidr, name <cidr>
    like or
      like native-string
      like native-null
    flex true
