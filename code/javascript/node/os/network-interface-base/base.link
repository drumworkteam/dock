
load @drumwork/moon/code/javascript/base/native-boolean
  take form native-boolean

load @drumwork/moon/code/javascript/base/native-null
  take form native-null

load @drumwork/moon/code/javascript/base/native-string
  take form native-string

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form network-interface-base
  name <NetworkInterfaceBase>
  take address, name <address>
    like native-string
    lock free
  take netmask, name <netmask>
    like native-string
    lock free
  take mac, name <mac>
    like native-string
    lock free
  take internal, name <internal>
    like native-boolean
    lock free
  take cidr, name <cidr>
    like sink-link-text
      like native-string
      like native-null
    lock free
