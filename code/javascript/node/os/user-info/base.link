
load @tunebond/moon
  take form like

load @tunebond/nest/code/javascript
  take form native-number

form user-info, name <UserInfo>
  head t

  link username, name <username>
    like t
    flex true
  link uid, name <uid>
    like native-number
    flex true
  link gid, name <gid>
    like native-number
    flex true
  link shell, name <shell>
    like t
    flex true
  link homedir, name <homedir>
    like t
    flex true
