
load @drumwork/moon/code/javascript/base/error
  take form error

load @drumwork/moon/code/javascript/base/native-null
  take form native-null

load @drumwork/moon/code/javascript/base/native-string
  take form native-string

load @drumwork/moon/code/javascript/base/native-void
  take form native-void

load @drumwork/moon/code/javascript/node/readline/completer-result
  take form completer-result

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form async-completer
  name <AsyncCompleter>
  like task
    take line, like native-string
    take callback
      like task
        take err
          like sink-link-text
            like native-null
            like error
          void take
        take result, like completer-result
          void take
        free seed, like native-void
    free seed, like native-void
