
bear @treesurf/nest/code/javascript/node/repl/repl-options
bear @treesurf/nest/code/javascript/node/repl/repl-eval
bear @treesurf/nest/code/javascript/node/repl/repl-writer
bear @treesurf/nest/code/javascript/node/repl/repl-command-action
bear @treesurf/nest/code/javascript/node/repl/repl-command
bear @treesurf/nest/code/javascript/node/repl/repl-server
bear @treesurf/nest/code/javascript/node/repl/recoverable

load @treesurf/nest/code/javascript/base/inspect-options
  take form inspect-options

load @treesurf/nest/code/javascript/base/native-string
  take form native-string

load @treesurf/nest/code/javascript/base/native-symbol
  take form native-symbol

load @treesurf/nest/code/javascript/base/repl-server
  take form repl-server

load @treesurf/nest/code/javascript/node/repl/repl-options
  take form repl-options

load @treesurf/nest/code/javascript/node/repl/repl-writer
  take form repl-writer

load @treesurf/nest/code/form/and
  take form and
    save form and-link-text

load @treesurf/nest/code/form/form
  take form form
    save form form-link-text

load @treesurf/nest/code/form/or
  take form or
    save form or-link-text

host writer, name <writer>
  like and-link-text
    like repl-writer
    like form-link-text
      take options, name <options>
        like inspect-options
        lock false

host repl-mode-sloppy, name <REPL_MODE_SLOPPY>
  like native-symbol
    cool true

host repl-mode-strict, name <REPL_MODE_STRICT>
  like native-symbol
    cool true

task start, name <start>
  take options
    like or-link-text
      like native-string
      like repl-options
    void true
  free seed, like repl-server
