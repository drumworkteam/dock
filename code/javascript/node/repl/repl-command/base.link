
load @treesurf/moon
  take form or

load @drumwork/nest/code/javascript
  take form string
    save form native-string
  take form undefined
    save form native-undefined

load @drumwork/nest/code/javascript/node/repl
  take form repl-command-action

form repl-command, name <REPLCommand>
  link help, name <help>
    like or
      like native-string
      like native-undefined
