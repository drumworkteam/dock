
load @treesurf/nest/code/javascript/base/async-iterable
  take form async-iterable

load @treesurf/nest/code/javascript/base/promise
  take form promise

load @treesurf/nest/code/form/like
  take form like
    save form like-link-text

load @treesurf/nest/code/form/task
  take form task
    save form task-link-text

form pipeline-destination-promise-function, name <PipelineDestinationPromiseFunction>
  head t, like like-link-text
  head p, like like-link-text

  like task-link-text
    take source
      like async-iterable
        like t
    free seed
      like promise
        like p
