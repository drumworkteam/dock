
load @drumwork/land/code/javascript/base/error
  take form error

load @drumwork/land/code/javascript/base/native-boolean
  take form native-boolean

load @drumwork/land/code/javascript/base/native-null
  take form native-null

load @drumwork/land/code/javascript/base/native-string
  take form native-string

load @drumwork/land/code/javascript/base/native-undefined
  take form native-undefined

load @drumwork/land/code/javascript/base/native-void
  take form native-void

load @drumwork/land/code/javascript/node/globals/uint8-array
  take form uint8-array

load @drumwork/land/code/javascript/node/tls/secure-context
  take form secure-context

load @drumwork/seed/code/form/list
  take form list
    save form list-link-text

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form common-connection-options
  name <CommonConnectionOptions>
  take secure-context
    name <secureContext>
    like sink-link-text
      like secure-context
      like native-undefined
    lock free
  take enable-trace, name <enableTrace>
    like sink-link-text
      like native-boolean
      like native-undefined
    lock free
  take request-cert, name <requestCert>
    like sink-link-text
      like native-boolean
      like native-undefined
    lock free
  take alpn-protocols
    name <ALPNProtocols>
    like sink-link-text
      like list-link-text
        like native-string
      like list-link-text
        like uint8-array
      like uint8-array
      like native-undefined
    lock free
  take sni-callback, name <SNICallback>
    like sink-link-text
      like task
        take servername, like native-string
        take cb
          like task
            take err
              like sink-link-text
                like error
                like native-null
            take ctx, like secure-context
              void take
            free seed, like native-void
        free seed, like native-void
      like native-undefined
    lock free
  take reject-unauthorized
    name <rejectUnauthorized>
    like sink-link-text
      like native-boolean
      like native-undefined
    lock free
