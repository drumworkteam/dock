
load @tunebond/moon
  take form or

load @tunebond/bolt/code/javascript
  take form native-string
  take form native-undefined

load @tunebond/bolt/code/javascript/node/buffer
  take form buffer

form key-object, name <KeyObject>
  link pem, name <pem>
    like or
      like native-string
      like buffer
    flex true
  link passphrase, name <passphrase>
    like or
      like native-string
      like native-undefined
    flex true
