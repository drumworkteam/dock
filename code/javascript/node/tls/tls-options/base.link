form tls-options, name <TlsOptions>
  base secure-context-options
  base common-connection-options
  base net-server-opts

  take handshake-timeout
    name <handshakeTimeout>
    like sink-link-text
      like native-number
      like native-undefined
    lock free
  take session-timeout
    name <sessionTimeout>
    like sink-link-text
      like native-number
      like native-undefined
    lock free
  take ticket-keys, name <ticketKeys>
    like sink-link-text
      like buffer
      like native-undefined
    lock free
  take psk-identity-hint
    name <pskIdentityHint>
    like sink-link-text
      like native-string
      like native-undefined
    lock free

  task psk-callback, name <pskCallback>
    take socket, like tls-socket
    take identity, like native-string
    free seed
      like sink-link-text
        like data-view
        like node-js-typed-array
        like native-null
