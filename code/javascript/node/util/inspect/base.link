
load @treesurf/moon
  take form tuple

load @drumwork/nest/code/javascript
  take form node-js-dict
  take form number
    save form native-number
  take form string
    save form native-string
  take form symbol
    save form native-symbol

load @drumwork/nest/code/javascript/node/util
  take form inspect-options
  take form style

host colors, name <colors>
  like node-js-dict
    like tuple
      like native-number
      like native-number

host styles, name <styles>
  slot self
  walk link-name-like-list
    loan like style
    take name
    take k
    beam self
      take {name}
        like native-string

host default-options, name <defaultOptions>
  like inspect-options

host repl-defaults, name <replDefaults>
  like inspect-options

host custom, name <custom>
  like native-symbol
    cool true
