
load @drumwork/moon/code/javascript/base/native-boolean
  take form native-boolean

load @drumwork/moon/code/javascript/base/native-number
  take form native-number

load @drumwork/moon/code/javascript/base/native-undefined
  take form native-undefined

load @drumwork/moon/code/javascript/node/vm/base-options
  take form base-options

load @drumwork/seed/code/form/or
  take form or
    save form or-link-text

form running-script-options, name <RunningScriptOptions>
  base base-options

  take display-errors, name <displayErrors>
    like or-link-text
      like native-boolean
      like native-undefined
    lock false
  take timeout, name <timeout>
    like or-link-text
      like native-number
      like native-undefined
    lock false
  take break-on-sigint, name <breakOnSigint>
    like or-link-text
      like native-boolean
      like native-undefined
    lock false
  take microtask-mode, name <microtaskMode>
    like or-link-text
      text <afterEvaluate>
      like native-undefined
    lock false
