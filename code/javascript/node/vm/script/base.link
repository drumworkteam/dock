
load @drumwork/land/code/javascript/base/native-any
  take form native-any

load @drumwork/land/code/javascript/base/native-boolean
  take form native-boolean

load @drumwork/land/code/javascript/base/native-string
  take form native-string

load @drumwork/land/code/javascript/base/native-undefined
  take form native-undefined

load @drumwork/land/code/javascript/node/buffer/buffer
  take form buffer

load @drumwork/land/code/javascript/node/vm/context
  take form context

load @drumwork/land/code/javascript/node/vm/running-script-options
  take form running-script-options

load @drumwork/land/code/javascript/node/vm/script-options
  take form script-options

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form script, name <Script>
  take cached-data-produced, name <cachedDataProduced>
    like sink-link-text
      like native-boolean
      like native-undefined
  take cached-data-rejected, name <cachedDataRejected>
    like sink-link-text
      like native-boolean
      like native-undefined
  take cached-data, name <cachedData>
    like sink-link-text
      like buffer
      like native-undefined

  task run-in-context
    name <runInContext>
    take contextified-object, like context
    take options, like running-script-options
      void take
    free seed, like native-any

  task run-in-new-context
    name <runInNewContext>
    take context-object, like context
      void take
    take options, like running-script-options
      void take
    free seed, like native-any

  task run-in-this-context
    name <runInThisContext>
    take options, like running-script-options
      void take
    free seed, like native-any

  task create-cached-data
    name <createCachedData>
    free seed, like buffer

form script-constructor
  hook make
    take code, like native-string
    take options, like script-options
      void take
