
load @treesurf/seed/code/javascript/base/native-any
  take form native-any

load @treesurf/seed/code/javascript/base/native-boolean
  take form native-boolean

load @treesurf/seed/code/javascript/base/native-string
  take form native-string

load @treesurf/seed/code/javascript/base/native-undefined
  take form native-undefined

load @treesurf/seed/code/javascript/node/buffer/buffer
  take form buffer

load @treesurf/seed/code/javascript/node/vm/context
  take form context

load @treesurf/seed/code/javascript/node/vm/running-script-options
  take form running-script-options

load @treesurf/seed/code/javascript/node/vm/script-options
  take form script-options

load @treesurf/seed/code/form/or
  take form or
    save form or-link-text

form script, name <Script>
  take cached-data-produced, name <cachedDataProduced>
    like or-link-text
      like native-boolean
      like native-undefined
  take cached-data-rejected, name <cachedDataRejected>
    like or-link-text
      like native-boolean
      like native-undefined
  take cached-data, name <cachedData>
    like or-link-text
      like buffer
      like native-undefined

  task run-in-context, name <runInContext>
    take contextified-object, like context
    take options, like running-script-options
      void true
    free seed, like native-any

  task run-in-new-context, name <runInNewContext>
    take context-object, like context
      void true
    take options, like running-script-options
      void true
    free seed, like native-any

  task run-in-this-context, name <runInThisContext>
    take options, like running-script-options
      void true
    free seed, like native-any

  task create-cached-data, name <createCachedData>
    free seed, like buffer

form script-constructor
  hook make
    take code, like native-string
    take options, like script-options
      void true
