
load @tunebond/moon
  take form or
  take form task

load @tunebond/bolt/code/javascript
  take form native-boolean
  take form native-number
  take form native-string
  take form native-undefined
  take form native-void

form zlib, name <Zlib>
  link bytes-read, name <bytesRead>
    like native-number
  link bytes-written, name <bytesWritten>
    like native-number
  link shell, name <shell>
    like or
      like native-boolean
      like native-string
      like native-undefined
    flex true

  task close, name <close>
    take callback
      like task
        free result, like native-void
      void true
    free seed, like native-void

  task flush, name <flush>
    take kind, like native-number
      void true
    take callback
      like task
        free result, like native-void
      void true
    free seed, like native-void

  task flush, name <flush>
    take callback
      like task
        free result, like native-void
      void true
    free seed, like native-void
