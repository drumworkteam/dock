
load @treesurf/moon
  take form or
  take form task

load @drumwork/nest/code/javascript
  take form boolean
    save form native-boolean
  take form number
    save form native-number
  take form string
    save form native-string
  take form undefined
    save form native-undefined
  take form void
    save form native-void

form zlib, name <Zlib>
  link bytes-read, name <bytesRead>
    like native-number
  link bytes-written, name <bytesWritten>
    like native-number
  link shell, name <shell>
    like or
      like native-boolean
      like native-string
      like native-undefined
    flex true

  task close, name <close>
    take callback
      like task
        free result, like native-void
      void true
    free seed, like native-void

  task flush, name <flush>
    take kind, like native-number
      void true
    take callback
      like task
        free result, like native-void
      void true
    free seed, like native-void

  task flush, name <flush>
    take callback
      like task
        free result, like native-void
      void true
    free seed, like native-void
