
fetch ..
  fetch /string
    catch form string
  fetch /function
    catch form function

field number

field integer
  form number

field integer-32
  form integer

field decimal

field unsigned-integer
  form integer

task parse-decimal
  base string, form string

  call call-base
    bind object, text <window>
    bind method, text <parseFloat>
    bind factor, link string
    turn seed

task parse-int
  base string, form string

  call call-base
    bind object, text <window>
    bind method, text <parseInt>
    bind factor, link string
    turn seed

task parse-number
  base value

  call call-base
    bind object, text <window>
    bind method, text <Number>
    bind factor, link value
    turn seed

task get-max
  base start, form number
  base end, form number

  call call-base
    bind object, text <Math>
    bind method, text <max>
    bind factor, link start
    bind factor, link end
    turn seed

