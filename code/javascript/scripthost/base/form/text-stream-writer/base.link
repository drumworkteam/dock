
load @drumwork/land/code/javascript/base/form/native-number
  take form native-number

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/base/form/native-void
  take form native-void

load @drumwork/land/code/javascript/scripthost/base/form/text-stream-base
  take form text-stream-base

form text-stream-writer
  base text-stream-base

  task write
    like task
      take s, like native-string
      free seed, like native-void

  task write-blank-lines
    like task
      take int-lines, like native-number
      free seed, like native-void

  task write-line
    like task
      take s, like native-string
      free seed, like native-void
