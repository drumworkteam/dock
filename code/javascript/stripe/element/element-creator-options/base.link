
load @drumwork/moon/code/javascript/base/native-array
  take form native-array

load @drumwork/moon/code/javascript/base/native-string
  take form native-string

load @drumwork/moon/code/javascript/base/native-undefined
  take form native-undefined

load @drumwork/moon/code/javascript/stripe/element/font-config-element
  take form font-config-element

load @drumwork/moon/code/javascript/stripe/element/font-css-element
  take form font-css-element

load @drumwork/seed/code/form/or
  take form or
    save form or-link-text

form element-creator-options, name <ElementCreatorOptions>
  take fonts, name <fonts>
    like or-link-text
      like native-array
        like font-css-element
      like native-array
        like font-config-element
      like native-undefined
    lock false
  take locale, name <locale>
    like or-link-text
      text <auto>
      text <da>
      text <de>
      text <en>
      text <es>
      text <fi>
      text <fr>
      text <it>
      text <ja>
      text <no>
      text <nl>
      text <sv>
      text <zh>
      like native-string
      like native-undefined
    lock false
