
load @drumwork/seed/code/form/number
  take form unsigned-integer-32

load @drumwork/seed/code/form/task
  take form task

task create
  link timeout, form unsigned-integer-32
  link handle, form task
  bear unsigned-integer-32

  call call-base
    bind object, <window>
    bind method, <setTimeout>
    bind factor, loan handle
    bind factor, loan timeout

task remove
  link id, form unsigned-integer-32

  call call-base
    bind object, <window>
    bind method, <clearTimeout>
    bind factor, loan id
