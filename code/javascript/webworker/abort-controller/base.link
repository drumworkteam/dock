
load @tunebond/nest/code/javascript/native
  take face native-void

load @tunebond/nest/code/javascript/webworker
  take face abort-signal

load @tunebond/nest/code/javascript/dom/abort-controller
  take face abort-controller

face abort-controller, name <AbortController>
  link signal, name <signal>
    like abort-signal

  task abort, name <abort>
    free seed, like native-void
