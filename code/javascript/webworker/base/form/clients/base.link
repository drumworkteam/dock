
load @drumwork/land/code/javascript/base/form/native-null
  take form native-null

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/base/form/native-undefined
  take form native-undefined

load @drumwork/land/code/javascript/base/form/native-void
  take form native-void

load @drumwork/land/code/javascript/es5/base/form/promise
  take form promise

load @drumwork/land/code/javascript/es5/base/form/readonly-array
  take form readonly-array

load @drumwork/land/code/javascript/webworker/base/form/client
  take form client

load @drumwork/land/code/javascript/webworker/base/form/client-query-options
  take form client-query-options

load @drumwork/land/code/javascript/webworker/base/form/window-client
  take form window-client

load @drumwork/seed/code/form/form/link
  take form form-link
    save form form-link-link-text

load @drumwork/seed/code/form/like
  take form like
    save form like-link-text

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

load @drumwork/seed/code/form/test
  take form test
    save form test-link-text

form clients
  task claim
    like task
      free seed
        like promise
          like native-void

  task get
    like task
      take id, like native-string
      free seed
        like promise
          like sink-link-text
            like client
            like native-undefined

  task match-all
    like task
      head t, like like-link-text
        base client-query-options
      take options, like t
        void take
      free seed
        like promise
          like readonly-array
            like test-link-text
              stem roll
                like form-link-link-text
                  like t
                  text <type>
                text <window>
                hook take
                  like window-client
                hook free
                  like client

  task open-window
    like task
      take url, like native-string
      free seed
        like promise
          like sink-link-text
            like window-client
            like native-null
