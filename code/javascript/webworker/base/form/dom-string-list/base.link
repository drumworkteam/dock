
load @drumwork/land/code/javascript/base/form/native-boolean
  take form native-boolean

load @drumwork/land/code/javascript/base/form/native-null
  take form native-null

load @drumwork/land/code/javascript/base/form/native-number
  take form native-number

load @drumwork/land/code/javascript/base/form/native-string
  take form native-string

load @drumwork/land/code/javascript/dom/iterable/form/dom-string-list
  take form dom-string-list

load @drumwork/seed/code/form/sink
  take form sink
    save form sink-link-text

form dom-string-list
  take length, like native-number

  task contains
    like task
      take string, like native-string
      free seed, like native-boolean

  task item
    like task
      take index, like native-number
      free seed
        like sink-link-text
          like native-string
          like native-null

  cite index, like native-number
    like native-string
