
load @tunebond/moon
  take suit any
  take suit or

load @tunebond/nest/code/javascript/native
  take face native-boolean
  take face native-null
  take face native-string

load @tunebond/nest/code/javascript/es5
  take face array-buffer
  take face promise
  take face uint8-array

load @tunebond/nest/code/javascript/webworker
  take face blob
  take face form-data
  take face readable-stream

load @tunebond/nest/code/javascript/dom/body
  take face body

face body, name <Body>
  link body, name <body>
    like or
      like readable-stream
        like uint8-array
      like native-null
  link body-used, name <bodyUsed>
    like native-boolean

  task array-buffer, name <arrayBuffer>
    free seed
      like promise
        like array-buffer

  task blob, name <blob>
    free seed
      like promise
        like blob

  task form-data, name <formData>
    free seed
      like promise
        like form-data

  task json, name <json>
    free seed
      like promise
        like any

  task text, name <text>
    free seed
      like promise
        like native-string
