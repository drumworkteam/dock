
load @tunebond/bolt/code/javascript/native
  take face native-boolean

load @tunebond/bolt/code/javascript/webworker
  take face image-smoothing-quality

load @tunebond/bolt/code/javascript/dom/canvas-image-smoothing
  take face canvas-image-smoothing

face canvas-image-smoothing, name <CanvasImageSmoothing>
  link image-smoothing-enabled, name <imageSmoothingEnabled>
    like native-boolean
    flex true
  link image-smoothing-quality, name <imageSmoothingQuality>
    like image-smoothing-quality
    flex true
