
load @tunebond/bolt/code/javascript/webworker
  take face crypto-key

load @tunebond/bolt/code/javascript/dom/crypto-key-pair
  take face crypto-key-pair

face crypto-key-pair, name <CryptoKeyPair>
  link private-key, name <privateKey>
    like crypto-key
    flex true
  link public-key, name <publicKey>
    like crypto-key
    flex true
