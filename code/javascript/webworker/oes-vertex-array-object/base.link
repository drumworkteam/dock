
load @treesurf/moon
  take form or

load @treesurf/nest/code/javascript/native
  take form null
    save form native-null
  take form void
    save form native-void

load @treesurf/nest/code/javascript/webworker
  take form g-lboolean
  take form g-lenum
  take form web-gl-vertex-array-object-oes

load @treesurf/nest/code/javascript/dom/oes-vertex-array-object
  take form oes-vertex-array-object

form oes-vertex-array-object, name <OES_vertex_array_object>
  link vertex-array-binding-oes, name <VERTEX_ARRAY_BINDING_OES>
    like g-lenum

  task bind-vertex-array-oes, name <bindVertexArrayOES>
    take array-object
      like or
        like web-gl-vertex-array-object-oes
        like native-null
    free seed, like native-void

  task create-vertex-array-oes, name <createVertexArrayOES>
    free seed
      like or
        like web-gl-vertex-array-object-oes
        like native-null

  task delete-vertex-array-oes, name <deleteVertexArrayOES>
    take array-object
      like or
        like web-gl-vertex-array-object-oes
        like native-null
    free seed, like native-void

  task is-vertex-array-oes, name <isVertexArrayOES>
    take array-object
      like or
        like web-gl-vertex-array-object-oes
        like native-null
    free seed, like g-lboolean
