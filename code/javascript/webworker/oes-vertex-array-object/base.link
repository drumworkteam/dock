
load @tunebond/moon
  take suit or

load @tunebond/nest/code/javascript/native
  take face native-null
  take face native-void

load @tunebond/nest/code/javascript/webworker
  take face g-lboolean
  take face g-lenum
  take face web-gl-vertex-array-object-oes

load @tunebond/nest/code/javascript/dom/oes-vertex-array-object
  take face oes-vertex-array-object

face oes-vertex-array-object, name <OES_vertex_array_object>
  link vertex-array-binding-oes, name <VERTEX_ARRAY_BINDING_OES>
    like g-lenum

  task bind-vertex-array-oes, name <bindVertexArrayOES>
    take array-object
      like or
        like web-gl-vertex-array-object-oes
        like native-null
    free seed, like native-void

  task create-vertex-array-oes, name <createVertexArrayOES>
    free seed
      like or
        like web-gl-vertex-array-object-oes
        like native-null

  task delete-vertex-array-oes, name <deleteVertexArrayOES>
    take array-object
      like or
        like web-gl-vertex-array-object-oes
        like native-null
    free seed, like native-void

  task is-vertex-array-oes, name <isVertexArrayOES>
    take array-object
      like or
        like web-gl-vertex-array-object-oes
        like native-null
    free seed, like g-lboolean
