
load @tunebond/moon
  take suit any
  take suit like

load @tunebond/bolt/code/javascript/native
  take face native-void

load @tunebond/bolt/code/javascript/es5
  take face promise

load @tunebond/bolt/code/javascript/webworker
  take face readable-stream-default-read-result
  take face readable-stream-generic-reader

load @tunebond/bolt/code/javascript/dom/readable-stream-default-reader
  take face readable-stream-default-reader

face readable-stream-default-reader, name <ReadableStreamDefaultReader>
  base readable-stream-generic-reader

  head r
    back any

  task read, name <read>
    free seed
      like promise
        like readable-stream-default-read-result
          like r

  task release-lock, name <releaseLock>
    free seed, like native-void
