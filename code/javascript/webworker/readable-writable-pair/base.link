
load @treesurf/moon
  take face any
  take face like

load @treesurf/nest/code/javascript/webworker
  take face readable-stream
  take face writable-stream

load @treesurf/nest/code/javascript/dom/readable-writable-pair
  take face readable-writable-pair

face readable-writable-pair, name <ReadableWritablePair>
  head r
    back any
  head w
    back any

  link readable, name <readable>
    like readable-stream
      like r
    flex true
  link writable, name <writable>
    like writable-stream
      like w
    flex true
