
load @tunebond/bolt/code/javascript/native
  take face native-number

load @tunebond/bolt/code/javascript/webworker
  take face algorithm
  take face big-integer

load @tunebond/bolt/code/javascript/dom/rsa-key-gen-params
  take face rsa-key-gen-params

face rsa-key-gen-params, name <RsaKeyGenParams>
  base algorithm

  link modulus-length, name <modulusLength>
    like native-number
    flex true
  link public-exponent, name <publicExponent>
    like big-integer
    flex true
