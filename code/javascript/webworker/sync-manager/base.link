
load @tunebond/nest/code/javascript
  take face array

load @tunebond/nest/code/javascript/native
  take face native-string
  take face native-void

load @tunebond/nest/code/javascript/es5
  take face promise

face sync-manager, name <SyncManager>
  task get-tags, name <getTags>
    free seed
      like promise
        like array
          like native-string

  task register, name <register>
    take tag, like native-string
    free seed
      like promise
        like native-void
