
load @treesurf/nest/code/javascript
  take form array

load @treesurf/nest/code/javascript/native
  take form string
    save form native-string
  take form void
    save form native-void

load @treesurf/nest/code/javascript/es5
  take form promise

form sync-manager, name <SyncManager>
  task get-tags, name <getTags>
    free seed
      like promise
        like array
          like native-string

  task register, name <register>
    take tag, like native-string
    free seed
      like promise
        like native-void
