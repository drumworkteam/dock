
load @tunebond/bolt/code/javascript/native
  take face native-boolean

load @tunebond/bolt/code/javascript/webworker
  take face value-type

load @tunebond/bolt/code/javascript/dom/global-descriptor
  take face global-descriptor

face global-descriptor, name <GlobalDescriptor>
  link mutable, name <mutable>
    like native-boolean
    flex true
  link value, name <value>
    like value-type
    flex true
