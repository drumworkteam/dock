
load @tunebond/moon
  take suit or

load @tunebond/bolt/code/javascript/native
  take face native-null
  take face native-number
  take face native-void

load @tunebond/bolt/code/javascript/es5
  take face function

load @tunebond/bolt/code/javascript/dom/table
  take face table

face table, name <Table>
  link length, name <length>
    like native-number

  task get, name <get>
    take index, like native-number
    free seed
      like or
        like function
        like native-null

  task grow, name <grow>
    take delta, like native-number
    free seed, like native-number

  task set, name <set>
    take index, like native-number
    take value
      like or
        like function
        like native-null
    free seed, like native-void
