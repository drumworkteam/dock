
load @treesurf/moon
  take suit or

load @treesurf/nest/code/javascript/native
  take face native-boolean
  take face native-null
  take face native-string

load @treesurf/nest/code/javascript/es5
  take face promise

load @treesurf/nest/code/javascript/webworker
  take face client
  take face visibility-state

face window-client, name <WindowClient>
  base client

  link focused, name <focused>
    like native-boolean
  link visibility-state, name <visibilityState>
    like visibility-state

  task focus, name <focus>
    free seed
      like promise
        like window-client

  task navigate, name <navigate>
    take url, like native-string
    free seed
      like promise
        like or
          like window-client
          like native-null
