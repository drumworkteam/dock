
load @treesurf/moon
  take face any
  take face like
  take face or

load @treesurf/nest/code/javascript/native
  take face native-null
  take face native-number
  take face native-undefined
  take face native-void

load @treesurf/nest/code/javascript/es5
  take face promise

load @treesurf/nest/code/javascript/dom/writable-stream-default-writer
  take face writable-stream-default-writer

face writable-stream-default-writer, name <WritableStreamDefaultWriter>
  head w
    back any

  link closed, name <closed>
    like promise
      like native-undefined
  link desired-size, name <desiredSize>
    like or
      like native-number
      like native-null
  link ready, name <ready>
    like promise
      like native-undefined

  task abort, name <abort>
    take reason, like any
      void true
    free seed
      like promise
        like native-void

  task close, name <close>
    free seed
      like promise
        like native-void

  task release-lock, name <releaseLock>
    free seed, like native-void

  task write, name <write>
    take chunk, like w
    free seed
      like promise
        like native-void
