
load @treesurf/moon
  take form any
  take form like
  take form or

load @treesurf/nest/code/javascript/native
  take form native-null
  take form native-number
  take form native-undefined
  take form native-void

load @treesurf/nest/code/javascript/es5
  take form promise

load @treesurf/nest/code/javascript/dom/writable-stream-default-writer
  take form writable-stream-default-writer

form writable-stream-default-writer, name <WritableStreamDefaultWriter>
  head w
    back any

  link closed, name <closed>
    like promise
      like native-undefined
  link desired-size, name <desiredSize>
    like or
      like native-number
      like native-null
  link ready, name <ready>
    like promise
      like native-undefined

  task abort, name <abort>
    take reason, like any
      void true
    free seed
      like promise
        like native-void

  task close, name <close>
    free seed
      like promise
        like native-void

  task release-lock, name <releaseLock>
    free seed, like native-void

  task write, name <write>
    take chunk, like w
    free seed
      like promise
        like native-void
