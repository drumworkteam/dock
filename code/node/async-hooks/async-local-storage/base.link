
load @nerdbond/bolt/code/javascript
  take form array
  take form native-undefined
  take form native-void
  take form t

load @nerdbond/moon
  take form any
  take form like
  take form or
  take form task

form async-local-storage, name <AsyncLocalStorage>
  task disable, name <disable>
    like native-void

  task get-store, name <getStore>
    like or
      like t
      like native-undefined

  task run, name <run>
    like r

    head r
    head t-args
      base array
        like any

    take store, like t
    take callback
      like task
        like r

        take args, like t-args
          rest true
    take args, like t-args
      rest true

  task exit, name <exit>
    like r

    head r
    head t-args
      base array
        like any

    take callback
      like task
        like r

        take args, like t-args
          rest true
    take args, like t-args
      rest true

  task enter-with, name <enterWith>
    like native-void

    take store, like t
