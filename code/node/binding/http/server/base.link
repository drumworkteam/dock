
form server
  base listening
  cast check-continue
  cast check-expectation
  cast client-error
  cast close
  cast connect
  cast connection
  cast request
  cast upgrade

task close
  server.close([callback])

task get-headers-timeout
  server.headersTimeout

task listen
  server.listen()

task get-is-listening
  server.listening

task get-max-headers-count
  server.maxHeadersCount

task get-request-timeout
  server.requestTimeout

task set-timeout
  server.setTimeout([msecs][, callback])

task get-max-requests-per-socket
  server.maxRequestsPerSocket

task get-timeout
  server.timeout

task get-keep-alive-timeout
  server.keepAliveTimeout
