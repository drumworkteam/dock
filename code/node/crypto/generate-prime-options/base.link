
load @treesurf/moon
  take form or

load @tunebond/bolt/code/javascript
  take form native-boolean
  take form native-undefined

load @tunebond/bolt/code/node/crypto
  take form large-number-like

form generate-prime-options, name <GeneratePrimeOptions>
  link add, name <add>
    like or
      like large-number-like
      like native-undefined
    flex true
  link rem, name <rem>
    like or
      like large-number-like
      like native-undefined
    flex true
  link safe, name <safe>
    like or
      like native-boolean
      like native-undefined
    flex true
  link bigint, name <bigint>
    like or
      like native-boolean
      like native-undefined
    flex true
