
load @termsurf/moon
  take form like
  take form or

load @termsurf/bolt/code/javascript
  take form native-string

load @termsurf/bolt/code/node/buffer
  take form buffer

form key-pair-sync-result, name <KeyPairSyncResult>
  head t1
    base or
      like native-string
      like buffer
  head t2
    base or
      like native-string
      like buffer

  link public-key, name <publicKey>
    like t1
    flex true
  link private-key, name <privateKey>
    like t2
    flex true
