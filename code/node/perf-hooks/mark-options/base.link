
load @treesurf/moon
  take form or
  take form unknown

load @tunebond/bolt/code/javascript
  take form native-number
  take form native-undefined

form mark-options, name <MarkOptions>
  link detail, name <detail>
    like or
      like unknown
      like native-undefined
    flex true
  link start-time, name <startTime>
    like or
      like native-number
      like native-undefined
    flex true
