
load @treesurf/moon
  take form or

load @tunebond/bolt/code/javascript
  take form native-bigint
  take form native-number
  take form native-void

load @tunebond/bolt/code/node/perf-hooks
  take form histogram

form recordable-histogram, name <RecordableHistogram>
  base histogram

  task record, name <record>
    like native-void

    take val
      like or
        like native-number
        like native-bigint

  task record-delta, name <recordDelta>
    like native-void
