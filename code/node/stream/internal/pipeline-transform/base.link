
load @tunebond/bolt/code/javascript
  take form array
  take form async-iterable
  take form iterable
  take form node-js-read-write-stream
  take form st

load @tunebond/moon
  take form any
  take form like
  take form or
  take form task
  take form test

load @tunebond/bolt/code/node/stream/internal
  take form pipeline-transform-source

form pipeline-transform, name <PipelineTransform>
  head s
    base pipeline-transform-source
      like any
  head u

  like or
    like node-js-read-write-stream
    like task
      like async-iterable
        like u

      take source
        like test
          stem roll
            like s
            like task
              like or
                like iterable
                  name st
                like async-iterable
                  name st

              take args
                like array
                  like any
                rest true
            hook true
              like async-iterable
                like st
            hook false
              like s