
load @tunebond/moon
  take form or

load @tunebond/bolt/code/javascript
  take form native-number
  take form native-string
  take form native-undefined

form ephemeral-key-info, name <EphemeralKeyInfo>
  link type, name <type>
    like native-string
    flex true
  link name, name <name>
    like or
      like native-string
      like native-undefined
    flex true
  link size, name <size>
    like native-number
    flex true
